  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#BluetoothAudio">
    <rdfs:label xml:lang="en">Bluetooth audio</rdfs:label>
    <rdfs:comment xml:lang="en">Bluetooth is a wireless protocol utilizing short-range communications technology facilitating data transmission over short distances from fixed and mobile devices, creating wireless personal area networks (refers to audio).</rdfs:comment>
    <skos:inScheme>
      <skos:ConceptScheme rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#AudioInterfaceType"/>
    </skos:inScheme>
  </skos:Concept>
  <rdf:Description rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#hasAudioInterfaceType">
    <rdfs:comment xml:lang="en">Defines the type of interfaces used to transfer audio signals.</rdfs:comment>
    <rdfs:label xml:lang="en">audio interface type</rdfs:label>
    <rdfs:range rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#AudioInterfaceType"/>
  </rdf:Description>
