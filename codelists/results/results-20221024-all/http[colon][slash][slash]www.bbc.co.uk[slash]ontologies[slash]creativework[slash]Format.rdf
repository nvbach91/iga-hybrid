    <rdfs:comment xml:lang="en-gb">Used to represent that the primary format of a CreativeWork is Text</rdfs:comment>
    <skos:inScheme>
      <skos:ConceptScheme rdf:about="http://www.bbc.co.uk/ontologies/creativework/Format"/>
    </skos:inScheme>
  </skos:Concept>
  <rdf:Description rdf:about="http://www.bbc.co.uk/ontologies/creativework/primaryFormat">
    <rdfs:comment xml:lang="en-gb">Associates the Creative Work with the Primary Format instance</rdfs:comment>
    <rdfs:label xml:lang="en-gb">primaryFormat</rdfs:label>
    <rdfs:domain rdf:resource="http://www.bbc.co.uk/ontologies/creativework/CreativeWork"/>
