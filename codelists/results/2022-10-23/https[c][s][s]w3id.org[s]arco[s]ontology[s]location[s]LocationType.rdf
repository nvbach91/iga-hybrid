        <rdfs:label xml:lang="en">Location Type</rdfs:label>
        <rdfs:comment xml:lang="en">This class represents the type of location of a cultural property.</rdfs:comment>
      </skos:ConceptScheme>
    </rdfs:range>
  </owl:ObjectProperty>
