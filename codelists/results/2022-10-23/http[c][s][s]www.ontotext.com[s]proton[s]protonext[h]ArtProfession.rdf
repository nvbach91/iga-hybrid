  <owl:Ontology rdf:about="http://www.ontotext.com/proton/protonext"/>
  <skos:Concept rdf:about="http://www.ontotext.com/proton/protonext#ComicsCreator">
    <rdfs:isDefinedBy rdf:resource="http://www.ontotext.com/proton/protonext"/>
    <rdfs:label xml:lang="en">Comics creator </rdfs:label>
    <rdfs:comment xml:lang="en">The profession of comics creator </rdfs:comment>
    <skos:inScheme>
      <skos:ConceptScheme rdf:about="http://www.ontotext.com/proton/protonext#ArtProfession">
        <rdfs:isDefinedBy rdf:resource="http://www.ontotext.com/proton/protonext"/>
        <rdfs:label xml:lang="en">Sportsman</rdfs:label>
        <rdfs:comment xml:lang="en">A profession in the area of art.</rdfs:comment>
      </skos:ConceptScheme>
    </skos:inScheme>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ontotext.com/proton/protonext#Musician">
    <rdfs:isDefinedBy rdf:resource="http://www.ontotext.com/proton/protonext"/>
    <rdfs:label xml:lang="en">Musician</rdfs:label>
    <rdfs:comment xml:lang="en">Any musician, a profession of writing, performing, or making music. (Wikipedia)</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ontotext.com/proton/protonext#ArtProfession"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ontotext.com/proton/protonext#Writer">
    <rdfs:isDefinedBy rdf:resource="http://www.ontotext.com/proton/protonext"/>
    <rdfs:label xml:lang="en">Writer</rdfs:label>
    <rdfs:comment xml:lang="en">Any writer, a profession of producing literary content. (Wikipedia)</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ontotext.com/proton/protonext#ArtProfession"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ontotext.com/proton/protonext#Actor">
    <rdfs:isDefinedBy rdf:resource="http://www.ontotext.com/proton/protonext"/>
    <rdfs:label xml:lang="en">Actor</rdfs:label>
    <rdfs:comment xml:lang="en">Any actor acts in a dramatic production and who works in film, television, theatre, or radio in that capacity. (Wikipedia)</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ontotext.com/proton/protonext#ArtProfession"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ontotext.com/proton/protonext#Comedian">
    <rdfs:isDefinedBy rdf:resource="http://www.ontotext.com/proton/protonext"/>
    <rdfs:label xml:lang="en">Comedian</rdfs:label>
    <rdfs:comment xml:lang="en">The profession of a comedian</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ontotext.com/proton/protonext#ArtProfession"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ontotext.com/proton/protonext#Artist">
    <rdfs:isDefinedBy rdf:resource="http://www.ontotext.com/proton/protonext"/>
    <rdfs:label xml:lang="en">Artist</rdfs:label>
    <rdfs:comment xml:lang="en">Any artist a profession engaging in a broad spectrum of activities related to creating art, practicing the arts and/or demonstrating an art. (Wikipedia)</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ontotext.com/proton/protonext#ArtProfession"/>
  </skos:Concept>
