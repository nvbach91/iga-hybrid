<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dcmit="http://purl.org/dc/dcmitype/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:voaf="http://purl.org/vocommons/voaf#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:vann="http://purl.org/vocab/vann/">
  <owl:Ontology rdf:about="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
  <skos:Concept rdf:about="http://www.w3.org/2007/uwa/context/location.owl#Feature_ALTITUDE">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Altitude Feature</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Indicates that the location provider is capable of determining the altitude</rdfs:comment>
    <skos:inScheme>
      <skos:ConceptScheme rdf:about="http://www.w3.org/2007/uwa/context/common.owl#Feature">
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Feature</rdfs:label>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >This class represents features, which are distinguishing characteristics of an item</rdfs:comment>
      </skos:ConceptScheme>
    </skos:inScheme>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#Feature_MONOCHROME_DISPLAY">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >MonochromeDisplay</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This feature indicates that a display is monochrome</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.w3.org/2007/uwa/context/common.owl#Feature"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.w3.org/2007/uwa/context/location.owl#Feature_SPEED">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Speed Feature</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This feature indicates that a location provider is capable of determining the speed</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.w3.org/2007/uwa/context/common.owl#Feature"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.w3.org/2007/uwa/context/software.owl#Feature_VIRTUALKEYBOARD">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Virtual Keyboard</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Virtual keyboard feature indicates that exists a virtual keyboard that facilitiates user input</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.w3.org/2007/uwa/context/common.owl#Feature"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#Feature_COLOR_DISPLAY">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Color Display</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This feature indicates that color display is supported</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.w3.org/2007/uwa/context/common.owl#Feature"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.w3.org/2007/uwa/context/location.owl#Feature_COURSE">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Course Feature</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This feature indicates that the location provider supports course determination</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.w3.org/2007/uwa/context/common.owl#Feature"/>
  </skos:Concept>
</rdf:RDF>
