    <rdfs:label>partitive article</rdfs:label>
    <rdfs:comment>Article expressing a part or quantity.</rdfs:comment>
    <skos:inScheme>
      <skos:ConceptScheme rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#ArticlePOS">
