  <owl:Ontology rdf:about="https://saref.etsi.org/saref4ehaw/"/>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4ehaw/hasHabit">
    <rdfs:comment xml:lang="en">The habits of a user and a patient (as sub-class of user it also inherits habit), e.g. smoking or overeating.</rdfs:comment>
    <rdfs:label xml:lang="en">has habit</rdfs:label>
    <rdfs:domain rdf:resource="https://saref.etsi.org/saref4ehaw/User"/>
    <rdfs:range>
