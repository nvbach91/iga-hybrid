        <rdfs:isDefinedBy rdf:resource="https://saref.etsi.org/saref4ehaw/"/>
        <rdfs:label xml:lang="en">Habit</rdfs:label>
        <rdfs:comment xml:lang="en">Defined for users (that can in particular be patients) habits modelling, e.g. smoking, alcohol drinking, overeating, undereating...</rdfs:comment>
      </skos:ConceptScheme>
    </rdfs:range>
  </owl:ObjectProperty>
  <skos:Concept rdf:about="https://saref.etsi.org/saref4ehaw/AlcoholDrinking">
