  <owl:Ontology rdf:about="https://w3id.org/idsa/core"/>
  <owl:ObjectProperty rdf:about="https://w3id.org/idsa/core/queryScope">
    <rdfs:comment xml:lang="en">Refers to the scope property of a query message, see ids:QueryScope.</rdfs:comment>
    <rdfs:label xml:lang="en">queryScope</rdfs:label>
    <rdfs:domain rdf:resource="https://w3id.org/idsa/core/QueryMessage"/>
    <rdfs:range>
