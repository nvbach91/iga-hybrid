<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dcmit="http://purl.org/dc/dcmitype/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:voaf="http://purl.org/vocommons/voaf#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:vann="http://purl.org/vocab/vann/">
  <owl:Ontology rdf:about="https://w3id.org/iot-tta/"/>
  <owl:ObjectProperty rdf:about="https://w3id.org/iot-tta/#hasFunctionality">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >hasFunctionality (Object property): a data center offers cloud services (as functionality)</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >hasFunctionality</rdfs:label>
    <rdfs:domain rdf:resource="https://w3id.org/iot-tta/#Data_center"/>
    <rdfs:range>
      <skos:ConceptScheme rdf:about="https://w3id.org/iot-tta/#Cloud_service">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/iot-tta/"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Cloud_service</rdfs:label>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Cloud services perform communications between devices, data gathering and processing.</rdfs:comment>
      </skos:ConceptScheme>
    </rdfs:range>
  </owl:ObjectProperty>
  <owl:FunctionalProperty rdf:about="https://w3id.org/iot-tta/#hasService">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >hasService (Object property): specifies cloud services provided by data centers</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >hasService</rdfs:label>
    <rdfs:domain rdf:resource="https://w3id.org/iot-tta/#Data_center"/>
    <rdfs:range rdf:resource="https://w3id.org/iot-tta/#Cloud_service"/>
  </owl:FunctionalProperty>
  <skos:Concept rdf:about="https://w3id.org/iot-tta/#Avoid_synchronization">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/iot-tta/"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >avoid_synchronization</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >avoid_synchronization (Individual): a Cloud service</rdfs:comment>
    <skos:inScheme rdf:resource="https://w3id.org/iot-tta/#Cloud_service"/>
  </skos:Concept>
  <skos:Concept rdf:about="https://w3id.org/iot-tta/#Maintain_configuration">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/iot-tta/"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >maintain_configuration</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >maintain_configuration (Individual): a Cloud service</rdfs:comment>
    <skos:inScheme rdf:resource="https://w3id.org/iot-tta/#Cloud_service"/>
  </skos:Concept>
  <skos:Concept rdf:about="https://w3id.org/iot-tta/#Centralized_service">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/iot-tta/"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >centralized_service</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >centralized_service (Individual): a Cloud service</rdfs:comment>
    <skos:inScheme rdf:resource="https://w3id.org/iot-tta/#Cloud_service"/>
  </skos:Concept>
</rdf:RDF>
