    <skos:inScheme rdf:resource="https://saref.etsi.org/saref4watr/WaterMeterProperty"/>
  </skos:Concept>
  <skos:Concept rdf:about="https://saref.etsi.org/saref4watr/BatteryOperatingTime">
    <rdfs:isDefinedBy rdf:resource="https://saref.etsi.org/saref4watr/"/>
    <rdfs:label xml:lang="en">Battery operating time</rdfs:label>
    <rdfs:comment xml:lang="en">The time for which the battery has been operating since the last charge.</rdfs:comment>
    <skos:inScheme rdf:resource="https://saref.etsi.org/saref4watr/WaterMeterProperty"/>
  </skos:Concept>
  <skos:Concept rdf:about="https://saref.etsi.org/saref4watr/BatteryRemainingTime">
    <rdfs:isDefinedBy rdf:resource="https://saref.etsi.org/saref4watr/"/>
    <rdfs:label xml:lang="en">Battery remaining time</rdfs:label>
