<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dcmit="http://purl.org/dc/dcmitype/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:voaf="http://purl.org/vocommons/voaf#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:vann="http://purl.org/vocab/vann/">
  <owl:Ontology rdf:about="https://w3id.org/iot-tta/"/>
  <owl:ObjectProperty rdf:about="https://w3id.org/iot-tta/#request">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >request (Object property): Particle cloud service will forward messages with values “1” and “0” to Thingspeak as a subscriber. A Webhook (a new HTTP request to Thingspeak) will result after passing values to the Particle cloud.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >request</rdfs:label>
    <rdfs:domain rdf:resource="https://w3id.org/iot-tta/#Particle_cloud"/>
    <rdfs:range>
      <skos:ConceptScheme rdf:about="https://w3id.org/iot-tta/#Subscriber">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/iot-tta/"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Subscriber</rdfs:label>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >A subscriber requests information about a specific topic in a publish/subscribe network and receives messages from more publishers. A subscription is a request sent by the subscriber to receive messages on a particular topic.</rdfs:comment>
      </skos:ConceptScheme>
    </rdfs:range>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/iot-tta/#flow_services">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >flow_services: Object property that ensures the flows and services from Particle Photon to the subscriber (Thingspeak)</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >flow_services</rdfs:label>
    <rdfs:domain rdf:resource="https://w3id.org/iot-tta/#Particle_Photon"/>
    <rdfs:range rdf:resource="https://w3id.org/iot-tta/#Subscriber"/>
  </owl:ObjectProperty>
  <skos:Concept rdf:about="https://w3id.org/iot-tta/#Thingspeak2">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/iot-tta/"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >thingspeak2</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >thingspeak2 (Individual): a Subscriber</rdfs:comment>
    <skos:inScheme rdf:resource="https://w3id.org/iot-tta/#Subscriber"/>
  </skos:Concept>
  <skos:Concept rdf:about="https://w3id.org/iot-tta/#Thingspeak1">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/iot-tta/"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >thingspeak1</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >thingspeak1 (Individual): a Subscriber</rdfs:comment>
    <skos:inScheme rdf:resource="https://w3id.org/iot-tta/#Subscriber"/>
  </skos:Concept>
</rdf:RDF>
