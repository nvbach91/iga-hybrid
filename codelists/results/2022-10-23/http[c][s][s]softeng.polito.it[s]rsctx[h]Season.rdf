        <rdfs:isDefinedBy rdf:resource="http://softeng.polito.it/rsctx"/>
        <rdfs:comment xml:lang="en">A season is a division of the year marked by changes in weather.</rdfs:comment>
      </skos:ConceptScheme>
    </rdfs:range>
  </owl:FunctionalProperty>
