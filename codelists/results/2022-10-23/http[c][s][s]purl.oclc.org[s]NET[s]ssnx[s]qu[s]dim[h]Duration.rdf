  <skos:Concept rdf:about="http://purl.oclc.org/NET/ssnx/cf/cf-property#age_of_stratospheric_air">
    <rdfs:isDefinedBy rdf:resource="http://purl.oclc.org/NET/ssnx/cf/cf-property"/>
    <j.0:generalQuantityKind rdf:resource="http://purl.oclc.org/NET/ssnx/qu/quantity#duration"/>
    <rdfs:label>age of stratospheric air</rdfs:label>
    <rdfs:comment>"Age of stratospheric air" means an estimate of the time since a parcel of stratospheric air was last in contact with the troposphere.</rdfs:comment>
    <skos:inScheme rdf:resource="http://purl.oclc.org/NET/ssnx/qu/dim#Duration"/>
  </skos:Concept>
