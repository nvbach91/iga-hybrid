  </skos:Concept>
  <skos:Concept rdf:about="http://www.ics.forth.gr/isl/VoIDWarehouse/VoID_Extension_Schema.owl#entityMatching">
    <rdfs:isDefinedBy rdf:resource="http://www.ics.forth.gr/isl/VoIDWarehouse/VoID_Extension_Schema.owl"/>
