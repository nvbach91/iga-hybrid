  <skos:Concept rdf:about="https://w3id.org/arco/ontology/cultural-event/Month">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/cultural-event"/>
    <rdfs:label xml:lang="en">Month</rdfs:label>
    <skos:inScheme rdf:resource="https://w3id.org/arco/ontology/cultural-event/TimePeriodMeasurementUnit"/>
