  <owl:Ontology rdf:about="https://w3id.org/idsa/core"/>
  <owl:ObjectProperty rdf:about="https://w3id.org/idsa/core/connectorDeployMode">
    <rdfs:comment xml:lang="en">Deply mode of a Connector in the Configuration Model</rdfs:comment>
    <rdfs:label xml:lang="en">connector deploy mode</rdfs:label>
    <rdfs:domain rdf:resource="https://w3id.org/idsa/core/ConfigurationModel"/>
    <rdfs:range>
      <skos:ConceptScheme rdf:about="https://w3id.org/idsa/core/ConnectorDeployMode">
