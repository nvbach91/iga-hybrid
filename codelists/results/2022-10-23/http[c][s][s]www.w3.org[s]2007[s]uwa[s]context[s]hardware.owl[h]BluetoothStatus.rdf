<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dcmit="http://purl.org/dc/dcmitype/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:voaf="http://purl.org/vocommons/voaf#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:vann="http://purl.org/vocab/vann/">
  <owl:Ontology rdf:about="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
  <owl:FunctionalProperty rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#bluetoothStatus">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This property indicates the Bluetooth Status of a device</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Bluetooth Status</rdfs:label>
    <rdfs:domain rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl#Device"/>
    <rdfs:range>
      <skos:ConceptScheme rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#BluetoothStatus">
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Bluetooth Status</rdfs:label>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >This class represents the possible statuses of Bluetooth in a device</rdfs:comment>
      </skos:ConceptScheme>
    </rdfs:range>
  </owl:FunctionalProperty>
  <skos:Concept rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#BluetoothStatus_HIDDEN">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Bluetooth Status Hidden</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This status indicates that the device cannot be discovered by other Bluetooth devices</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.w3.org/2007/uwa/context/hardware.owl#BluetoothStatus"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#BluetoothStatus_TEMP_DISCOVERABLE">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Bluetooth Status Temporarily Discoverable</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This status indicates that the device can be temporarily discovered by other Bluetooth devices</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.w3.org/2007/uwa/context/hardware.owl#BluetoothStatus"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#BluetoothStatus_DISCOVERABLE">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Bluetooth Discoverable</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This status indicates that the device can be discovered by other Bluetooth devices</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.w3.org/2007/uwa/context/hardware.owl#BluetoothStatus"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#BluetoothStatus_PAIRED">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Bluetooth Status Paired</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This status indicates that the device is paired to other Bluetooth device</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.w3.org/2007/uwa/context/hardware.owl#BluetoothStatus"/>
  </skos:Concept>
</rdf:RDF>
