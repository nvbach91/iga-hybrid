<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dcmit="http://purl.org/dc/dcmitype/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:voaf="http://purl.org/vocommons/voaf#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:vann="http://purl.org/vocab/vann/">
  <owl:Ontology rdf:about="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
  <owl:FunctionalProperty rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#pointingResolution">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This property represents the precision with which pointing can be carried out on a device</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Pointing Resolution</rdfs:label>
    <rdfs:domain rdf:resource="http://www.w3.org/2007/uwa/context/hardware.owl#DeviceHardware"/>
    <rdfs:range>
      <skos:ConceptScheme rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#PointingResolution">
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Pointing Resolution</rdfs:label>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >This class represents the precision with which the pointing can be achieved on a  device</rdfs:comment>
      </skos:ConceptScheme>
    </rdfs:range>
  </owl:FunctionalProperty>
  <skos:Concept rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#PointingResolution_CHAR">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Character Pointing Resolution</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The pointing device can select an individual character</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.w3.org/2007/uwa/context/hardware.owl#PointingResolution"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#PointingResolution_PIXEL">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Pixel Pointing Resolution</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The pointing device can select an individual pixel</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.w3.org/2007/uwa/context/hardware.owl#PointingResolution"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#PointingResolution_LINE">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Line Pointing Resolution</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The pointing device can select a line of text</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.w3.org/2007/uwa/context/hardware.owl#PointingResolution"/>
  </skos:Concept>
