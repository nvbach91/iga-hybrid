    <rdfs:label xml:lang="en">Auto white balance</rdfs:label>
    <rdfs:comment xml:lang="en">Ability to adapt of different adjustments of the intensities of the colors.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#WhiteBalance"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#IndoorWhiteBalance">
    <rdfs:isDefinedBy rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1"/>
    <rdfs:label xml:lang="en">Indoor white balance</rdfs:label>
    <rdfs:comment xml:lang="en">Ability to adapt of different adjustments of the intensities of the colors.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#WhiteBalance"/>
  </skos:Concept>
