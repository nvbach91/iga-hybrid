  <owl:Ontology rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1"/>
  <owl:ObjectProperty rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#hasWhiteBalance">
    <rdfs:comment xml:lang="en">Defines the global adjustment of the intensities of the colors.</rdfs:comment>
    <rdfs:label xml:lang="en">white balance</rdfs:label>
    <rdfs:range>
      <skos:ConceptScheme rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#WhiteBalance">
        <rdfs:isDefinedBy rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1"/>
        <rdfs:label xml:lang="en">White balance</rdfs:label>
        <rdfs:comment xml:lang="en">Is the global adjustment of the intensities of the colors (typically red, green, and blue primary colors). An important goal of this adjustment is to render specific colors - particularly neutral colors - correctly; hence, the general method is sometimes called gray balance, neutral balance, or white balance.

(This description is partly based on or reuses content from the Wikipedia article "White Balance" (http://en.wikipedia.org/wiki/White_Balance) and is licensed under the GNU Free Documentation License (http://www.gnu.org/copyleft/fdl.html).)</rdfs:comment>
      </skos:ConceptScheme>
    </rdfs:range>
  </owl:ObjectProperty>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#OutdoorWhiteBalance">
    <rdfs:isDefinedBy rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1"/>
    <rdfs:label xml:lang="en">Outdoor white balance</rdfs:label>
    <rdfs:comment xml:lang="en">Ability to adapt of different adjustments of the intensities of the colors.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#WhiteBalance"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#AutoWhiteBalance">
