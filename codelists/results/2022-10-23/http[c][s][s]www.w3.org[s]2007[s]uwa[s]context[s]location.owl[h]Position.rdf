    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Position</rdfs:label>
    <rdfs:domain rdf:resource="http://www.w3.org/2007/uwa/context/location.owl#Location"/>
    <rdfs:range rdf:resource="http://www.w3.org/2007/uwa/context/location.owl#Position"/>
  </owl:FunctionalProperty>
  <skos:Concept rdf:about="http://www.example.org/2007/uwa/context/examples.owl#Position1">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >An example position</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.w3.org/2007/uwa/context/location.owl#Position"/>
  </skos:Concept>
