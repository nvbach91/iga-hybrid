    <j.0:generalQuantityKind>
      <skos:Concept rdf:about="http://purl.oclc.org/NET/ssnx/qu/quantity#surfaceDensity">
        <skos:inScheme>
          <skos:ConceptScheme rdf:about="http://purl.oclc.org/NET/ssnx/qu/dim#SurfaceDensity">
