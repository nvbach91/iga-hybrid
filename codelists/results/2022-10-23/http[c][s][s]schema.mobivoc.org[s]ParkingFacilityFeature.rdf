<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dcmit="http://purl.org/dc/dcmitype/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:voaf="http://purl.org/vocommons/voaf#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:vann="http://purl.org/vocab/vann/">
  <owl:Ontology rdf:about="http://schema.mobivoc.org/"/>
  <rdf:Property rdf:about="http://schema.mobivoc.org/feature">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment>Indicates which feature the parking facility provides.</rdfs:comment>
    <rdfs:label xml:lang="en">feature</rdfs:label>
    <rdfs:domain rdf:resource="http://schema.mobivoc.org/ParkingFacility"/>
    <rdfs:range>
      <skos:ConceptScheme rdf:about="http://schema.mobivoc.org/ParkingFacilityFeature">
        <rdfs:isDefinedBy rdf:resource="http://schema.mobivoc.org/"/>
        <rdfs:label xml:lang="en">Parking facility feature</rdfs:label>
        <rdfs:comment>Features of the parking facility (e.g. parking for disabled people).</rdfs:comment>
      </skos:ConceptScheme>
    </rdfs:range>
  </rdf:Property>
  <skos:Concept rdf:about="http://schema.mobivoc.org/ParkingBarrier">
    <rdfs:isDefinedBy rdf:resource="http://schema.mobivoc.org/"/>
    <rdfs:label xml:lang="en">Parking barrier</rdfs:label>
    <rdfs:comment xml:lang="en">The parking facility has a parking barrier.</rdfs:comment>
    <skos:inScheme rdf:resource="http://schema.mobivoc.org/ParkingFacilityFeature"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://schema.mobivoc.org/SurveillanceBySecurityGuards">
    <rdfs:isDefinedBy rdf:resource="http://schema.mobivoc.org/"/>
    <rdfs:label xml:lang="en">surveillance by security guards</rdfs:label>
    <rdfs:comment xml:lang="en">The parking facility is surveilled by security guards.</rdfs:comment>
    <skos:inScheme rdf:resource="http://schema.mobivoc.org/ParkingFacilityFeature"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://schema.mobivoc.org/VideoSurveillance">
    <rdfs:isDefinedBy rdf:resource="http://schema.mobivoc.org/"/>
    <rdfs:label xml:lang="en">Video surveillance</rdfs:label>
    <rdfs:comment xml:lang="en">The parking facility is surveilled by video.</rdfs:comment>
    <skos:inScheme rdf:resource="http://schema.mobivoc.org/ParkingFacilityFeature"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://schema.mobivoc.org/PayStation">
    <rdfs:isDefinedBy rdf:resource="http://schema.mobivoc.org/"/>
    <rdfs:label xml:lang="en">Pay station</rdfs:label>
    <rdfs:comment xml:lang="en">The parking facility has a pay station.</rdfs:comment>
    <skos:inScheme rdf:resource="http://schema.mobivoc.org/ParkingFacilityFeature"/>
  </skos:Concept>
