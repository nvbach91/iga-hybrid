<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dcmit="http://purl.org/dc/dcmitype/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:voaf="http://purl.org/vocommons/voaf#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:vann="http://purl.org/vocab/vann/">
  <owl:Ontology rdf:about="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
  <owl:FunctionalProperty rdf:about="http://www.w3.org/2007/uwa/context/location.owl#lastKnownLocation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The last known location associated to a Delivery Context</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Last Known Location</rdfs:label>
    <rdfs:domain rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl#Environment"/>
    <rdfs:range>
      <skos:ConceptScheme rdf:about="http://www.w3.org/2007/uwa/context/location.owl#Location">
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Location</rdfs:label>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >This class represents all the information about a location</rdfs:comment>
      </skos:ConceptScheme>
    </rdfs:range>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="http://www.w3.org/2007/uwa/context/location.owl#currentLocation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The current location associated to the environment component of this Delivery Context</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Current Location</rdfs:label>
    <rdfs:domain rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl#Environment"/>
    <rdfs:range rdf:resource="http://www.w3.org/2007/uwa/context/location.owl#Location"/>
  </owl:FunctionalProperty>
  <skos:Concept rdf:about="http://www.example.org/2007/uwa/context/examples.owl#Place_Example">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
    <skos:inScheme rdf:resource="http://www.w3.org/2007/uwa/context/location.owl#Location"/>
  </skos:Concept>
</rdf:RDF>
