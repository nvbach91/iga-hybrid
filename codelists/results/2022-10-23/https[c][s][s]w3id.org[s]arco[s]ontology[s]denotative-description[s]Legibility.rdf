    <rdfs:label xml:lang="en">Decent Legibility</rdfs:label>
    <skos:inScheme rdf:resource="https://w3id.org/arco/ontology/denotative-description/Legibility"/>
  </skos:Concept>
  <skos:Concept rdf:about="https://w3id.org/arco/ontology/denotative-description/CompleteLegibility">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/denotative-description"/>
    <rdfs:label xml:lang="en">Complete legibility</rdfs:label>
    <skos:inScheme rdf:resource="https://w3id.org/arco/ontology/denotative-description/Legibility"/>
  </skos:Concept>
