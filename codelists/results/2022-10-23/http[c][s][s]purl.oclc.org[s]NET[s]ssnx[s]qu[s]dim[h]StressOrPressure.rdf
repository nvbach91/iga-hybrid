  <skos:Concept rdf:about="http://purl.oclc.org/NET/ssnx/cf/cf-property#surface_air_pressure">
    <rdfs:isDefinedBy rdf:resource="http://purl.oclc.org/NET/ssnx/cf/cf-property"/>
    <j.0:generalQuantityKind rdf:resource="http://purl.oclc.org/NET/ssnx/qu/quantity#pressure"/>
