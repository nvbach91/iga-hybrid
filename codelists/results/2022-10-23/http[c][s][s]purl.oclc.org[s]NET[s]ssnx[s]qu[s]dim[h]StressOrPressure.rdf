  <skos:Concept rdf:about="http://purl.oclc.org/NET/ssnx/cf/cf-property#downward_x_stress_at_sea_ice_base">
    <rdfs:isDefinedBy rdf:resource="http://purl.oclc.org/NET/ssnx/cf/cf-property"/>
