    <rdfs:label>air pressure anomaly</rdfs:label>
    <rdfs:comment>"anomaly" means difference from climatology.</rdfs:comment>
    <skos:inScheme rdf:resource="http://purl.oclc.org/NET/ssnx/qu/dim#StressOrPressure"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://purl.oclc.org/NET/ssnx/cf/cf-property#air_pressure_at_cloud_base">
    <rdfs:isDefinedBy rdf:resource="http://purl.oclc.org/NET/ssnx/cf/cf-property"/>
