  <skos:Concept rdf:about="http://purl.oclc.org/NET/ssnx/cf/cf-property#air_pressure">
    <rdfs:isDefinedBy rdf:resource="http://purl.oclc.org/NET/ssnx/cf/cf-property"/>
    <j.0:generalQuantityKind>
      <skos:Concept rdf:about="http://purl.oclc.org/NET/ssnx/qu/quantity#pressure">
