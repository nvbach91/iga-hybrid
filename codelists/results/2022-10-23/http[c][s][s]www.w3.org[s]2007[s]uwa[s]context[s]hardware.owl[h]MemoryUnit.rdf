<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dcmit="http://purl.org/dc/dcmitype/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:voaf="http://purl.org/vocommons/voaf#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:vann="http://purl.org/vocab/vann/">
  <owl:Ontology rdf:about="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
  <owl:ObjectProperty rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#extensionMemory">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This property identifies additional memory that is provided to a device, typically in the form of removable memory cards</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Extension Memory</rdfs:label>
    <rdfs:domain rdf:resource="http://www.w3.org/2007/uwa/context/hardware.owl#DeviceHardware"/>
    <rdfs:range>
      <skos:ConceptScheme rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#MemoryUnit">
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Memory Unit</rdfs:label>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >This class represents some memory used in a device. It might, for example, be built-in memory that is part of the device, or it might be removable memory on some form of storage card</rdfs:comment>
      </skos:ConceptScheme>
    </rdfs:range>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#memory">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This property represents the hardware memory component/s of a device</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Memory</rdfs:label>
    <rdfs:domain rdf:resource="http://www.w3.org/2007/uwa/context/hardware.owl#DeviceHardware"/>
    <rdfs:range rdf:resource="http://www.w3.org/2007/uwa/context/hardware.owl#MemoryUnit"/>
  </owl:ObjectProperty>
  <owl:FunctionalProperty rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#builtInMemory">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This property represents the memory built into the device and which is not removable during normal operation</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Built-in Memory</rdfs:label>
    <rdfs:domain rdf:resource="http://www.w3.org/2007/uwa/context/hardware.owl#DeviceHardware"/>
    <rdfs:range rdf:resource="http://www.w3.org/2007/uwa/context/hardware.owl#MemoryUnit"/>
  </owl:FunctionalProperty>
  <skos:Concept rdf:about="http://www.example.org/2007/uwa/context/examples.owl#MemoryUnit_Hyp_Dev_Card">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
    <skos:inScheme rdf:resource="http://www.w3.org/2007/uwa/context/hardware.owl#MemoryUnit"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.example.org/2007/uwa/context/examples.owl#MemoryUnit_Hyp_Dev_Internal">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
    <skos:inScheme rdf:resource="http://www.w3.org/2007/uwa/context/hardware.owl#MemoryUnit"/>
  </skos:Concept>
</rdf:RDF>
