<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dcmit="http://purl.org/dc/dcmitype/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:voaf="http://purl.org/vocommons/voaf#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:vann="http://purl.org/vocab/vann/">
  <owl:Ontology rdf:about="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
  <skos:Concept rdf:about="http://www.w3.org/2007/uwa/context/common.owl#OutputModality_AURAL">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Aural Output Modality</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This modality indicates the ability to use audio as an output modality</rdfs:comment>
    <skos:inScheme>
      <skos:ConceptScheme rdf:about="http://www.w3.org/2007/uwa/context/common.owl#OutputModality">
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Output Modality</rdfs:label>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >This class represents output modalities</rdfs:comment>
      </skos:ConceptScheme>
    </skos:inScheme>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.w3.org/2007/uwa/context/common.owl#OutputModality_TACTILE">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Tactile Output Modality</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This is a tactile modality in which user receives information from the system by means of the tac</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.w3.org/2007/uwa/context/common.owl#OutputModality"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.w3.org/2007/uwa/context/common.owl#OutputModality_VISUAL">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Visual Output Modality</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This is an output modality in which information is perceived visually by the user</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.w3.org/2007/uwa/context/common.owl#OutputModality"/>
  </skos:Concept>
