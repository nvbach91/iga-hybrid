  <skos:Concept rdf:about="http://purl.oclc.org/NET/ssnx/cf/cf-property#atmosphere_specific_convective_available_potential_energy">
    <rdfs:isDefinedBy rdf:resource="http://purl.oclc.org/NET/ssnx/cf/cf-property"/>
    <j.0:generalQuantityKind>
      <skos:Concept rdf:about="http://purl.oclc.org/NET/ssnx/qu/quantity#specificEnergy">
        <skos:inScheme>
          <skos:ConceptScheme rdf:about="http://purl.oclc.org/NET/ssnx/qu/dim#SpecificEnergy">
            <rdfs:isDefinedBy rdf:resource="http://purl.oclc.org/NET/ssnx/cf/cf-property"/>
