<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dcmit="http://purl.org/dc/dcmitype/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:voaf="http://purl.org/vocommons/voaf#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:vann="http://purl.org/vocab/vann/">
  <owl:Ontology rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1"/>
  <owl:ObjectProperty rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#hasAccessory">
    <rdfs:comment xml:lang="en">Defines subordinate or supplementary items.</rdfs:comment>
    <rdfs:label xml:lang="en">accessory</rdfs:label>
    <rdfs:range>
      <skos:ConceptScheme rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Accessory">
        <rdfs:isDefinedBy rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1"/>
        <rdfs:label xml:lang="en">Accessory</rdfs:label>
        <rdfs:comment xml:lang="en">A subordinate or supplementary item; an adjunct.</rdfs:comment>
      </skos:ConceptScheme>
    </rdfs:range>
  </owl:ObjectProperty>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#IR_Remote">
    <rdfs:isDefinedBy rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1"/>
    <rdfs:label xml:lang="en">IR remote control</rdfs:label>
    <rdfs:comment xml:lang="en">Remote control with a electromagnetic radiation whose wavelength is longer than that of visible light (400-700 nm), but shorter than that of terahertz radiation (3-300 µm) and microwaves (~30,000 µm). Infrared radiation spans roughly three orders of magnitude (750 nm and 1000 µm)</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Accessory"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Headphones">
    <rdfs:isDefinedBy rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1"/>
    <rdfs:label xml:lang="en">Headphones</rdfs:label>
    <rdfs:comment xml:lang="en">A pair of small loudspeakers, or less commonly a single speaker, with a way of holding them close to a user's ears and a means of connecting them to a signal source such as an audio amplifier, radio or CD player.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Accessory"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#DC_Cable">
    <rdfs:isDefinedBy rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1"/>
    <rdfs:label xml:lang="en">DC cable</rdfs:label>
    <rdfs:comment xml:lang="en">The mean amplitude of a waveform.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Accessory"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#AC_Cable">
    <rdfs:isDefinedBy rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1"/>
    <rdfs:label xml:lang="en">AC cable</rdfs:label>
    <rdfs:comment xml:lang="en">A net transfer of energy over a complete cycle of the alternating current waveform.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Accessory"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#USB_Cable">
    <rdfs:isDefinedBy rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1"/>
    <rdfs:label xml:lang="en">USB cable</rdfs:label>
    <rdfs:comment xml:lang="en">A serial bus standard to connect devices to a host computer.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Accessory"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#AV_Cable">
    <rdfs:isDefinedBy rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1"/>
    <rdfs:label xml:lang="en">AV cable</rdfs:label>
    <rdfs:comment xml:lang="en">A cable to carry video and audio signals from a device, to a tv; in digital or analog format.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Accessory"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#VideoComponent_Cable">
    <rdfs:isDefinedBy rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1"/>
    <rdfs:label xml:lang="en">Video component cable</rdfs:label>
    <rdfs:comment xml:lang="en">A video signal that has been split into two or more components.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Accessory"/>
  </skos:Concept>
</rdf:RDF>
