    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dcmit="http://purl.org/dc/dcmitype/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:voaf="http://purl.org/vocommons/voaf#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:vann="http://purl.org/vocab/vann/">
  <owl:Ontology rdf:about="https://w3id.org/iot-tta/"/>
  <owl:ObjectProperty rdf:about="https://w3id.org/iot-tta/#wire">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >wire (Object property): When the wire of a Particle Photon is connected, the Photon will send the value “1”, but if the wire is disconnected, the Photon will send the value “0”.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >wire</rdfs:label>
    <rdfs:domain rdf:resource="https://w3id.org/iot-tta/#Particle_Photon"/>
    <rdfs:range>
      <skos:ConceptScheme rdf:about="https://w3id.org/iot-tta/#Flow">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/iot-tta/"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Flow</rdfs:label>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Flow describes the Flow-Service-Quality semantics for a Particle Photon application that includes a Thingspeak subscriber, a WebHook, and a firmware program.</rdfs:comment>
      </skos:ConceptScheme>
    </rdfs:range>
  </owl:ObjectProperty>
  <skos:Concept rdf:about="https://w3id.org/iot-tta/#Connection2">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/iot-tta/"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >connection2</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >connection2 (Individual): a Flow with Object property assertions (read), Data property assertions (hasValueConnection)</rdfs:comment>
    <skos:inScheme rdf:resource="https://w3id.org/iot-tta/#Flow"/>
  </skos:Concept>
  <skos:Concept rdf:about="https://w3id.org/iot-tta/#Connection1">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/iot-tta/"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >connection1</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >connection1 (Individual): a Flow with Object property assertions (read), Data property assertions (hasValueConnection)</rdfs:comment>
    <skos:inScheme rdf:resource="https://w3id.org/iot-tta/#Flow"/>
  </skos:Concept>
