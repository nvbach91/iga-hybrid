<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dcmit="http://purl.org/dc/dcmitype/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:voaf="http://purl.org/vocommons/voaf#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:vann="http://purl.org/vocab/vann/">
  <owl:Ontology rdf:about="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
  <owl:ObjectProperty rdf:about="http://www.w3.org/2007/uwa/context/location.owl#supportedGeoCoordinateRS">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Indicates what are goegraphical coordinate systems supported by the location provider</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Geographical Coordinate Systems</rdfs:label>
    <rdfs:domain rdf:resource="http://www.w3.org/2007/uwa/context/location.owl#LocationProvider"/>
    <rdfs:range>
      <skos:ConceptScheme rdf:about="http://www.w3.org/2007/uwa/context/location.owl#GeoCoordinateRS">
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Geographical Coordinate Reference System</rdfs:label>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >This class represents a geographical coordinate system</rdfs:comment>
      </skos:ConceptScheme>
    </rdfs:range>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.w3.org/2007/uwa/context/location.owl#geoReferenceSystem">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This property represents the reference system in which are expressed the geographical coordinates</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Georeference System</rdfs:label>
    <rdfs:domain rdf:resource="http://www.w3.org/2007/uwa/context/location.owl#GeoCoordinates"/>
    <rdfs:range rdf:resource="http://www.w3.org/2007/uwa/context/location.owl#GeoCoordinateRS"/>
  </owl:ObjectProperty>
  <skos:Concept rdf:about="http://www.w3.org/2007/uwa/context/location.owl#GeoCoordinateRS_UTM">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >UTM</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The Universal Traversal Marcator coordinate reference system</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.w3.org/2007/uwa/context/location.owl#GeoCoordinateRS"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.w3.org/2007/uwa/context/location.owl#GeoCoordinateRS_WGS84">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >WGS 84</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The WGS 84 geographic coordinates reference system</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.w3.org/2007/uwa/context/location.owl#GeoCoordinateRS"/>
  </skos:Concept>
</rdf:RDF>
