<rdf:RDF
    xmlns:dct="http://purl.org/dc/terms/"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dcmit="http://purl.org/dc/dcmitype/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:voaf="http://purl.org/vocommons/voaf#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:vann="http://purl.org/vocab/vann/">
  <owl:Ontology rdf:about="http://dataid.dbpedia.org/ns/core#"/>
  <owl:ObjectProperty rdf:about="http://dataid.dbpedia.org/ns/core#validForAccessLevel">
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:comment xml:lang="en">Limits rights and actions an Agent is allwed to take depending on the AccessLevel chosen for this AuthorityEntityContext.</rdfs:comment>
    <rdfs:label xml:lang="en">valid for access level</rdfs:label>
    <rdfs:domain>
      <rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#AuthorityEntityContext">
        <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
      </rdf:Description>
    </rdfs:domain>
    <rdfs:range>
      <skos:ConceptScheme rdf:about="http://dataid.dbpedia.org/ns/core#AccessLevel">
        <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
      </skos:ConceptScheme>
    </rdfs:range>
  </owl:ObjectProperty>
  <owl:FunctionalProperty rdf:about="http://dataid.dbpedia.org/ns/core#hasAccessLevel">
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">defines the access rights for DataId related entities</rdfs:comment>
    <rdfs:label xml:lang="en">access level</rdfs:label>
    <rdfs:domain>
      <rdf:Description rdf:about="http://www.w3.org/ns/prov#Entity">
        <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
      </rdf:Description>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://dataid.dbpedia.org/ns/core#AccessLevel"/>
  </owl:FunctionalProperty>
  <skos:Concept rdf:about="http://dataid.dbpedia.org/ns/core#SemiPrivateAccess">
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">semi-private</rdfs:label>
    <rdfs:comment xml:lang="en">Limits the rights an agent has on a resource to read and modify.</rdfs:comment>
    <skos:inScheme rdf:resource="http://dataid.dbpedia.org/ns/core#AccessLevel"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://dataid.dbpedia.org/ns/core#NoAccess">
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">no access</rdfs:label>
    <rdfs:comment xml:lang="en">Disallows access to a resource for an agent.</rdfs:comment>
    <skos:inScheme rdf:resource="http://dataid.dbpedia.org/ns/core#AccessLevel"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://dataid.dbpedia.org/ns/core#PrivateAccess">
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">private</rdfs:label>
    <rdfs:comment xml:lang="en">Provides an agent with unrestricted access and all rights of a resource.</rdfs:comment>
    <skos:inScheme rdf:resource="http://dataid.dbpedia.org/ns/core#AccessLevel"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://dataid.dbpedia.org/ns/core#PublicAccess">
    <rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
    <rdfs:label xml:lang="en">public</rdfs:label>
    <rdfs:comment xml:lang="en">Limits the rights an agent has on a resource to read rights.</rdfs:comment>
    <skos:inScheme rdf:resource="http://dataid.dbpedia.org/ns/core#AccessLevel"/>
  </skos:Concept>
</rdf:RDF>
