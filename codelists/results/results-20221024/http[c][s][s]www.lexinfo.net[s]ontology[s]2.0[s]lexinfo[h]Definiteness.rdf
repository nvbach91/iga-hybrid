<rdf:RDF
    xmlns:dct="http://purl.org/dc/terms/"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dcmit="http://purl.org/dc/dcmitype/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:voaf="http://purl.org/vocommons/voaf#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:vann="http://purl.org/vocab/vann/">
  <owl:Ontology rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo"/>
  <owl:ObjectProperty rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#definiteness">
    <rdfs:isDefinedBy rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo"/>
    <rdfs:comment>Property about the possiblity to identify an entity.</rdfs:comment>
    <rdfs:label>definiteness</rdfs:label>
    <rdfs:range>
      <skos:ConceptScheme rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#Definiteness">
        <rdfs:isDefinedBy rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo"/>
      </skos:ConceptScheme>
    </rdfs:range>
  </owl:ObjectProperty>
  <skos:Concept rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#indefinite">
    <rdfs:isDefinedBy rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo"/>
    <rdfs:label>indefinite</rdfs:label>
    <rdfs:comment>Value related to an entity in a situation where the identification of this entity is not clear cut.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo#Definiteness"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#definite">
    <rdfs:isDefinedBy rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo"/>
    <rdfs:label>definite</rdfs:label>
    <rdfs:comment>Value referring to the capacity of identification of an entity.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo#Definiteness"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#fullArticle">
    <rdfs:isDefinedBy rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo"/>
    <rdfs:label>full article</rdfs:label>
    <rdfs:comment>For definiteness, when a specific form is the syntactic subject of the clause.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo#Definiteness"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#shortArticle">
    <rdfs:isDefinedBy rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo"/>
    <rdfs:label>short article</rdfs:label>
    <rdfs:comment>For definiteness, when a specific form is not the syntactic subject of the clause.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo#Definiteness"/>
  </skos:Concept>
</rdf:RDF>
