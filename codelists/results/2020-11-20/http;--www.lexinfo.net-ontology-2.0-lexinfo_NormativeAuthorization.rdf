<rdf:RDF
    xmlns:dct="http://purl.org/dc/terms/"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dcmit="http://purl.org/dc/dcmitype/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:voaf="http://purl.org/vocommons/voaf#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:vann="http://purl.org/vocab/vann/">
  <skos:Concept rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#preferredTerm">
    <rdfs:label xml:lang="ru">предпочтительный термин</rdfs:label>
    <rdfs:label>preferred term</rdfs:label>
    <rdfs:comment>A term rated according to the scale of a term acceptability rating as the primary term for a given concept.</rdfs:comment>
    <skos:inScheme>
      <skos:ConceptScheme rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#NormativeAuthorization"/>
    </skos:inScheme>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#standardizedTerm">
    <rdfs:label xml:lang="ru">стандартный термин</rdfs:label>
    <rdfs:label>standardized term</rdfs:label>
    <rdfs:comment>A term that has been standardized by a standardizing body.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo#NormativeAuthorization"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#deprecatedTerm">
    <rdfs:label xml:lang="ru">устаревший термин</rdfs:label>
    <rdfs:label>deprecated term</rdfs:label>
    <rdfs:comment>A term rated according to the scale of a term acceptability rating as undesired.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo#NormativeAuthorization"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#supersededTerm">
    <rdfs:label xml:lang="ru">вытесненный термин</rdfs:label>
    <rdfs:label>superseded term</rdfs:label>
    <rdfs:comment>A term that is no longer preferred or admitted.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo#NormativeAuthorization"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#admittedTerm">
    <rdfs:label xml:lang="ru">допустимый термин</rdfs:label>
    <rdfs:label>admitted term</rdfs:label>
    <rdfs:comment>A term rated according to the scale of a term acceptability rating as a synonym for a preferred term.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo#NormativeAuthorization"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#legalTerm">
    <rdfs:label xml:lang="ru">легальный термин</rdfs:label>
    <rdfs:label>legal term</rdfs:label>
    <rdfs:comment>A term that is legally defined and used in legally binding documents.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo#NormativeAuthorization"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#regulatedTerm">
    <rdfs:label xml:lang="ru">урегулированный термин</rdfs:label>
    <rdfs:label>regulated term</rdfs:label>
    <rdfs:comment>A term defined by law or government regulation.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo#NormativeAuthorization"/>
  </skos:Concept>
</rdf:RDF>
