<rdf:RDF
	xmlns:vann="http://purl.org/vocab/vann/"
	xmlns:voaf="http://purl.org/vocommons/voaf#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:dcterms="http://purl.org/dc/terms/"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:dcmit="http://purl.org/dc/dcmitype/"
	xmlns:sesame="http://www.openrdf.org/schema/sesame#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
	xmlns:fn="http://www.w3.org/2005/xpath-functions#"
	xmlns:foaf="http://xmlns.com/foaf/0.1/"
	xmlns:hint="http://www.bigdata.com/queryHints#"
	xmlns:bd="http://www.bigdata.com/rdf#"
	xmlns:bds="http://www.bigdata.com/rdf/search#">

<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds#ContractStatus">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds#contractStatusActive">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://purl.org/onto-ocds/ocds#ContractStatus"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds#ContractStatus">
	<rdfs:comment xml:lang="en">Contracts can move through multiple states. Releases over time may update the status of a contract.</rdfs:comment>
	<rdfs:label xml:lang="en">Codelist Contract Status</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds#contractStatusActive">
	<rdfs:comment xml:lang="en">This contract has been signed by all the parties, and is now legally in force.</rdfs:comment>
	<rdfs:label xml:lang="en">active</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds#contractStatus">
	<rdfs:range rdf:resource="http://purl.org/onto-ocds/ocds#ContractStatus"/>
	<rdfs:domain rdf:resource="http://purl.org/onto-ocds/ocds#Contract"/>
	<rdfs:label xml:lang="en">Contract status</rdfs:label>
	<rdfs:comment xml:lang="en">The current status of the contract. Drawn from the [contractStatus codelist](http://standard.open-contracting.org/latest/en/schema/codelists/#contract-status)</rdfs:comment>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds#ContractStatus">
	<rdfs:isDefinedBy rdf:resource="http://purl.org/onto-ocds/ocds"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds#contractStatusActive">
	<rdfs:isDefinedBy rdf:resource="http://purl.org/onto-ocds/ocds"/>
	<rdfs:label xml:lang="en">Active</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds#contractStatusCancelled">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://purl.org/onto-ocds/ocds#ContractStatus"/>
	<rdfs:comment xml:lang="en">This contract has been cancelled prior to being signed.</rdfs:comment>
	<rdfs:label xml:lang="en">cancelled</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/onto-ocds/ocds"/>
	<rdfs:label xml:lang="en">Cancelled</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds#contractStatusPending">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://purl.org/onto-ocds/ocds#ContractStatus"/>
	<rdfs:comment xml:lang="en">This contract has been proposed, but is not yet in force. It may be awaiting signature.</rdfs:comment>
	<rdfs:label xml:lang="en">pending</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/onto-ocds/ocds"/>
	<rdfs:label xml:lang="en">Pending</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds#contractStatusTerminated">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://purl.org/onto-ocds/ocds#ContractStatus"/>
	<rdfs:comment xml:lang="en">This contract was signed and in force, and has now come to a close. This may be due to successful completion of the contract, or may be early termination due to some non-completion.</rdfs:comment>
	<rdfs:label xml:lang="en">terminated</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/onto-ocds/ocds"/>
	<rdfs:label xml:lang="en">Terminated</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds#contractStatus">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
</rdf:Description>

</rdf:RDF>