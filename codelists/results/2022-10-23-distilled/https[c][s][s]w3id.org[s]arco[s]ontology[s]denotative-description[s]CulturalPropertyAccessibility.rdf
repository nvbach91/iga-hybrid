<rdf:Description rdf:about="https://w3id.org/arco/ontology/denotative-description">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/denotative-description/CulturalPropertyAccessibility">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/denotative-description/Accessible">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="https://w3id.org/arco/ontology/denotative-description/CulturalPropertyAccessibility"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/denotative-description/CulturalPropertyAccessibility">
	<rdfs:comment xml:lang="en">This class represents the cultural property accessibility, that is the possibility to reach or enter that cultural property, for example for cataloguing activities (e.g.: an immovable cultural property located in a walled area, a movable cultural property located on the top shelf, a totally accessible cultural property, etc.)</rdfs:comment>
	<rdfs:label xml:lang="en">Cultural property accessibility</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/denotative-description/Accessible">
	<rdfs:label xml:lang="en">Accessible</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/denotative-description/hasCulturalPropertyAccessibility">
	<rdfs:range rdf:resource="https://w3id.org/arco/ontology/denotative-description/CulturalPropertyAccessibility"/>
	<rdfs:domain rdf:resource="http://dati.beniculturali.it/cis/CulturalEntity"/>
	<rdfs:label xml:lang="en">has cultural property accessibility</rdfs:label>
	<rdfs:comment xml:lang="en">This property relates the cultural property to its accessibility, that is the possibility to reach or enter that cultural property, for example for cataloguing activities (e.g.: an immovable cultural property located in a walled area, a movable cultural property located on the top shelf, a totally accessible cultural property, etc.)</rdfs:comment>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/denotative-description/CulturalPropertyAccessibility">
	<rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/denotative-description"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/denotative-description/Accessible">
	<rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/denotative-description"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/denotative-description/Inaccessible">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="https://w3id.org/arco/ontology/denotative-description/CulturalPropertyAccessibility"/>
	<rdfs:label xml:lang="en">Inaccessible</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/denotative-description"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/denotative-description/PartiallyAccessible">
