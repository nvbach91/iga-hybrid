	xmlns:bds="http://www.bigdata.com/rdf/search#">

<rdf:Description rdf:about="http://www.w3.org/2007/uwa/context/deliverycontext.owl">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#MemoryUnit">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.example.org/2007/uwa/context/examples.owl#MemoryUnit_Hyp_Dev_Card">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://www.w3.org/2007/uwa/context/hardware.owl#MemoryUnit"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#MemoryUnit">
	<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents some memory used in a device. It might, for example, be built-in memory that is part of the device, or it might be removable memory on some form of storage card</rdfs:comment>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Memory Unit</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#builtInMemory">
	<rdfs:range rdf:resource="http://www.w3.org/2007/uwa/context/hardware.owl#MemoryUnit"/>
	<rdfs:domain rdf:resource="http://www.w3.org/2007/uwa/context/hardware.owl#DeviceHardware"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Built-in Memory</rdfs:label>
	<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This property represents the memory built into the device and which is not removable during normal operation</rdfs:comment>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#MemoryUnit">
	<rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.example.org/2007/uwa/context/examples.owl#MemoryUnit_Hyp_Dev_Card">
	<rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.example.org/2007/uwa/context/examples.owl#MemoryUnit_Hyp_Dev_Internal">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://www.w3.org/2007/uwa/context/hardware.owl#MemoryUnit"/>
	<rdfs:isDefinedBy rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#builtInMemory">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#extensionMemory">
	<rdfs:range rdf:resource="http://www.w3.org/2007/uwa/context/hardware.owl#MemoryUnit"/>
	<rdfs:domain rdf:resource="http://www.w3.org/2007/uwa/context/hardware.owl#DeviceHardware"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Extension Memory</rdfs:label>
	<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This property identifies additional memory that is provided to a device, typically in the form of removable memory cards</rdfs:comment>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.w3.org/2007/uwa/context/hardware.owl#memory">
	<rdfs:range rdf:resource="http://www.w3.org/2007/uwa/context/hardware.owl#MemoryUnit"/>
	<rdfs:domain rdf:resource="http://www.w3.org/2007/uwa/context/hardware.owl#DeviceHardware"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Memory</rdfs:label>
	<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This property represents the hardware memory component/s of a device</rdfs:comment>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
</rdf:Description>

