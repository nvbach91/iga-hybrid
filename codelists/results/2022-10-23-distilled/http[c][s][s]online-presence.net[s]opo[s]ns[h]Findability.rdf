<rdf:Description rdf:about="http://online-presence.net/opo/ns#Findability">
	<rdfs:comment xml:lang="en">The OnlinePresenceComponent used to represent the
			possibility to find contact details of an Agent in a public
			listing</rdfs:comment>
	<rdfs:label xml:lang="en">Findability</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://online-presence.net/opo/ns#ConstrainedFindability">
	<rdfs:comment xml:lang="en">The Agent's contact details cannot be found by anyone on
			the Web, but the findability is controled by some
			rules/policies.</rdfs:comment>
	<rdfs:label xml:lang="en">ConstrainedFindability</rdfs:label>
</rdf:Description>

