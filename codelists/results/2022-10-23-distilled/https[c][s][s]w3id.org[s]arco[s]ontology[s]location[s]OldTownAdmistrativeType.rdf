	xmlns:voaf="http://purl.org/vocommons/voaf#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:dcterms="http://purl.org/dc/terms/"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:dcmit="http://purl.org/dc/dcmitype/"
	xmlns:sesame="http://www.openrdf.org/schema/sesame#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
	xmlns:fn="http://www.w3.org/2005/xpath-functions#"
	xmlns:foaf="http://xmlns.com/foaf/0.1/"
	xmlns:hint="http://www.bigdata.com/queryHints#"
	xmlns:bd="http://www.bigdata.com/rdf#"
	xmlns:bds="http://www.bigdata.com/rdf/search#">

<rdf:Description rdf:about="https://w3id.org/arco/ontology/location">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/location/OldTownAdmistrativeType">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/location/ChiefTown">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="https://w3id.org/arco/ontology/location/OldTownAdmistrativeType"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/location/OldTownAdmistrativeType">
	<rdfs:comment xml:lang="en">This class represents the information clarifying the administrative character of the old town, which may be: a chief town, a town fraction, an incorporated area or a locality.</rdfs:comment>
	<rdfs:label xml:lang="en">Old town administrative type</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/location/ChiefTown">
	<rdfs:comment xml:lang="en">This individual represents a specific type of administrative reference that characterizes an old town, that is the chief town.</rdfs:comment>
	<rdfs:label xml:lang="en">Chief town</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/location/hasOldTownAdministrativeType">
	<rdfs:range rdf:resource="https://w3id.org/arco/ontology/location/OldTownAdmistrativeType"/>
	<rdfs:domain rdf:resource="https://w3id.org/arco/ontology/location/OldTown"/>
	<rdfs:label xml:lang="en">Old town administrative type</rdfs:label>
	<rdfs:comment xml:lang="en">This property connects a historical center with information related to its administrative type.</rdfs:comment>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/location/OldTownAdmistrativeType">
	<rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/location/ChiefTown">
	<rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/location/IncorporatedArea">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="https://w3id.org/arco/ontology/location/OldTownAdmistrativeType"/>
	<rdfs:comment xml:lang="en">This individual represents a specific type of administrative reference that characterizes the old town, that is the denomination of 'incorporated area'.</rdfs:comment>
	<rdfs:label xml:lang="en">Incorporated area</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/location/Locality">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="https://w3id.org/arco/ontology/location/OldTownAdmistrativeType"/>
	<rdfs:comment xml:lang="en">This individual represents a specific type of administrative reference that characterizes the old town, that is the locality.</rdfs:comment>
	<rdfs:label xml:lang="en">Locality</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/location/TownFraction">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="https://w3id.org/arco/ontology/location/OldTownAdmistrativeType"/>
	<rdfs:comment xml:lang="en">This individual represents a specific type of administrative reference that characterizes the old town, i.e. the town fraction.</rdfs:comment>
	<rdfs:label xml:lang="en">Town fraction</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location"/>
</rdf:Description>

</rdf:RDF>