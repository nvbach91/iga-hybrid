	<skos:inScheme rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo#AdpositionPOS"/>
	<rdfs:comment>Preposition that is a aggregation of words</rdfs:comment>
	<rdfs:label>compound preposition</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#postposition">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo#AdpositionPOS"/>
	<rdfs:comment>Adposition that appears at the end of the noun phrase.</rdfs:comment>
	<rdfs:label>postposition</rdfs:label>
