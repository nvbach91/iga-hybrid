<rdf:Description rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#circumposition">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo#AdpositionPOS"/>
	<rdfs:comment>Adposition which consists of two parts which are positioned ahead and after the adpositional foci.</rdfs:comment>
	<rdfs:label>circumposition</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.lexinfo.net/ontology/2.0/lexinfo#compoundPreposition">
