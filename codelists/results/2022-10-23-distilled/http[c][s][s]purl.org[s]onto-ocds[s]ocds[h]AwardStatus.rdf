<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds#AwardStatus">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds#awardStatusActive">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://purl.org/onto-ocds/ocds#AwardStatus"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds#AwardStatus">
	<rdfs:comment xml:lang="en">An award move through multiple states. Releases over time may update the status of an award.</rdfs:comment>
	<rdfs:label xml:lang="en">Codelist Award Status</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds#awardStatusActive">
	<rdfs:comment xml:lang="en">This award has been made, and is currently in force.</rdfs:comment>
	<rdfs:label xml:lang="en">Active</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds#awardStatus">
	<rdfs:range rdf:resource="http://purl.org/onto-ocds/ocds#AwardStatus"/>
	<rdfs:domain rdf:resource="http://purl.org/onto-ocds/ocds#Award"/>
	<rdfs:label>Award status</rdfs:label>
	<rdfs:comment>The current status of the award drawn from the [awardStatus codelist](http://standard.open-contracting.org/latest/en/schema/codelists/#award-status)</rdfs:comment>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds#AwardStatus">
	<rdfs:isDefinedBy rdf:resource="http://purl.org/onto-ocds/ocds"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds#awardStatusActive">
	<rdfs:isDefinedBy rdf:resource="http://purl.org/onto-ocds/ocds"/>
	<rdfs:label xml:lang="en">active</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds#awardStatusCancelled">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://purl.org/onto-ocds/ocds#AwardStatus"/>
	<rdfs:comment xml:lang="en">This award has been cancelled.</rdfs:comment>
	<rdfs:label xml:lang="en">Cancelled</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/onto-ocds/ocds"/>
	<rdfs:label xml:lang="en">cancelled</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds#awardStatusPending">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://purl.org/onto-ocds/ocds#AwardStatus"/>
	<rdfs:comment xml:lang="en">This award has been proposed, but is not yet in force. This may be due to a cooling off period, or some other process.</rdfs:comment>
	<rdfs:label xml:lang="en">Pending</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/onto-ocds/ocds"/>
	<rdfs:label xml:lang="en">pending</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds#awardStatusUnsuccessful">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://purl.org/onto-ocds/ocds#AwardStatus"/>
	<rdfs:comment xml:lang="en">This award could not be successfully made. If items or supplier details are included within the award section, then these narrow the scope of the unsuccessful award (i.e. the award of noted items, or an award to the noted supplier, was unsuccessful, but there may be other successful awards for different items listed in the tender, or to different suppliers).</rdfs:comment>
	<rdfs:label xml:lang="en">Unsuccessful</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/onto-ocds/ocds"/>
	<rdfs:label xml:lang="en">unsuccessful</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/onto-ocds/ocds#awardStatus">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
</rdf:Description>

</rdf:RDF>