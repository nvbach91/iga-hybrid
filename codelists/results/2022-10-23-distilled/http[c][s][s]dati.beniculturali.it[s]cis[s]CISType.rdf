	<rdfs:label xml:lang="en">has type</rdfs:label>
	<rdfs:comment xml:lang="en">The property is used to link a cultural institure or site to its type.</rdfs:comment>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
</rdf:Description>

