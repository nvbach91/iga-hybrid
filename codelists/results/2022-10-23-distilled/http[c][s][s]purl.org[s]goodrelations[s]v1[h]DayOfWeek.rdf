	<rdfs:label xml:lang="en">Saturday (day of week)</rdfs:label>
	<hasPrevious xmlns="http://purl.org/goodrelations/v1#" rdf:resource="http://purl.org/goodrelations/v1#Friday"/>
</rdf:Description>

