<rdf:Description rdf:about="http://vcharpenay.github.io/hto/hto.xml#sensor">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://vcharpenay.github.io/hto/hto.xml#HTag"/>
	<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">  Classifies a `point` as an input, AI/BI, or sensor.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://vcharpenay.github.io/hto/hto.xml"/>
</rdf:Description>

<rdf:Description rdf:about="http://vcharpenay.github.io/hto/hto.xml#sp">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://vcharpenay.github.io/hto/hto.xml#HTag"/>
	<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">  Classifies a `point` as a setpoint, soft point, or process control variable.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://vcharpenay.github.io/hto/hto.xml"/>
</rdf:Description>

