	<skos:inScheme rdf:resource="http://vcharpenay.github.io/hto/hto.xml#HTag"/>
	<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">  Equip which meters usage of `elec` energy, thermal energy, `gas`, `water` or `steam`.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://vcharpenay.github.io/hto/hto.xml"/>
</rdf:Description>

<rdf:Description rdf:about="http://vcharpenay.github.io/hto/hto.xml#mixed">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://vcharpenay.github.io/hto/hto.xml#HTag"/>
	<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">  Associated with the mixed air of an `ahu`.</rdfs:comment>
