	<skos:inScheme rdf:resource="http://vcharpenay.github.io/hto/hto.xml#HTag"/>
	<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">  Speed `point` of a `vfd` measured in "%" where 0% is off and 100% is the fastest speed.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://vcharpenay.github.io/hto/hto.xml"/>
