	<skos:inScheme rdf:resource="http://vcharpenay.github.io/hto/hto.xml#HTag"/>
	<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">  Point associated with the measurement or control of steam. This tag is often paired with `entering` versus `leaving` versus `delta`. It may also be paired with `meter` to indicate a steam meter.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://vcharpenay.github.io/hto/hto.xml"/>
</rdf:Description>

<rdf:Description rdf:about="http://vcharpenay.github.io/hto/hto.xml#temp">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://vcharpenay.github.io/hto/hto.xml#HTag"/>
	<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">  Temperature measured in Â°C or Â°F.</rdfs:comment>
	<rdfs:isDefinedBy rdf:resource="http://vcharpenay.github.io/hto/hto.xml"/>
</rdf:Description>

<rdf:Description rdf:about="http://vcharpenay.github.io/hto/hto.xml#valve">
