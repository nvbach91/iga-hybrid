	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:dcmit="http://purl.org/dc/dcmitype/"
	xmlns:sesame="http://www.openrdf.org/schema/sesame#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
	xmlns:fn="http://www.w3.org/2005/xpath-functions#"
	xmlns:foaf="http://xmlns.com/foaf/0.1/"
	xmlns:hint="http://www.bigdata.com/queryHints#"
	xmlns:bd="http://www.bigdata.com/rdf#"
	xmlns:bds="http://www.bigdata.com/rdf/search#">

<rdf:Description rdf:about="https://w3id.org/arco/ontology/context-description">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/context-description/TitleType">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/context-description/AlternativeTitle">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="https://w3id.org/arco/ontology/context-description/TitleType"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/context-description/TitleType">
	<rdfs:comment xml:lang="en">This class represents the type of an object title (e.g. original, alternative)</rdfs:comment>
	<rdfs:label xml:lang="en">Title Type</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/context-description/AlternativeTitle">
	<rdfs:comment xml:lang="en">Represents subject's alternative title.</rdfs:comment>
	<rdfs:label xml:lang="en">Alternative title</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/context-description/hasTitleType">
	<rdfs:range rdf:resource="https://w3id.org/arco/ontology/context-description/TitleType"/>
	<rdfs:domain rdf:resource="https://w3id.org/arco/ontology/context-description/Title"/>
	<rdfs:label xml:lang="en">has title type</rdfs:label>
	<rdfs:comment xml:lang="en">This property relates a cultural entity's title to its title type.</rdfs:comment>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/context-description/TitleType">
	<rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/context-description"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/context-description/AlternativeTitle">
	<rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/context-description"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/context-description/AttributedTitle">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="https://w3id.org/arco/ontology/context-description/TitleType"/>
	<rdfs:comment xml:lang="en">Represents an attributed title of a photograph, that is a title taken from archival, bibliographic, oral or iconographic sources, or created based on picture contents.</rdfs:comment>
	<rdfs:label xml:lang="en">Attributed title</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/context-description"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/context-description/MainTitle">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="https://w3id.org/arco/ontology/context-description/TitleType"/>
	<rdfs:comment xml:lang="en">This individual represents publication main title.</rdfs:comment>
	<rdfs:label xml:lang="en">Main title</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/context-description"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/context-description/ParallelTitle">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="https://w3id.org/arco/ontology/context-description/TitleType"/>
	<rdfs:comment xml:lang="en">I represents a parallel title, that is main title translated into other languages and presented as equivalent.</rdfs:comment>
	<rdfs:label xml:lang="en">Parallel title</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/context-description"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/context-description/ProperTitle">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="https://w3id.org/arco/ontology/context-description/TitleType"/>
	<rdfs:comment xml:lang="en">It represents the title proper to subject of a print, an engraved matrix or a photograph.</rdfs:comment>
	<rdfs:label xml:lang="en">Proper title</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/context-description"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/context-description/SecondaryTitle">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="https://w3id.org/arco/ontology/context-description/TitleType"/>
	<rdfs:comment xml:lang="en">This individual represents publication's secondary title.</rdfs:comment>
	<rdfs:label xml:lang="en">Secondary title</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/context-description"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/context-description/HistoricizedTitle">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="https://w3id.org/arco/ontology/context-description/TitleType"/>
	<rdfs:label xml:lang="en">Historicized title</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/context-description"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/arco/ontology/context-description/OriginalTitle">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="https://w3id.org/arco/ontology/context-description/TitleType"/>
	<rdfs:label xml:lang="en">Original title</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/context-description"/>
</rdf:Description>

</rdf:RDF>