<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
	xmlns:vann="http://purl.org/vocab/vann/"
	xmlns:voaf="http://purl.org/vocommons/voaf#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:dcterms="http://purl.org/dc/terms/"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:dcmit="http://purl.org/dc/dcmitype/"
	xmlns:sesame="http://www.openrdf.org/schema/sesame#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
	xmlns:fn="http://www.w3.org/2005/xpath-functions#"
	xmlns:foaf="http://xmlns.com/foaf/0.1/"
	xmlns:hint="http://www.bigdata.com/queryHints#"
	xmlns:bd="http://www.bigdata.com/rdf#"
	xmlns:bds="http://www.bigdata.com/rdf/search#">

<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
</rdf:Description>

<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#Agent">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
</rdf:Description>

<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#GuestAgent">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://dataid.dbpedia.org/ns/core#Agent"/>
</rdf:Description>

<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#Agent">
	<rdfs:comment xml:lang="en">Any type of agent without a specific function.</rdfs:comment>
	<rdfs:label xml:lang="en">Agent</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#associatedAgent">
	<rdfs:range rdf:resource="http://dataid.dbpedia.org/ns/core#Agent"/>
	<rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Entity"/>
	<rdfs:label xml:lang="en">associated agent</rdfs:label>
	<rdfs:comment xml:lang="en">some agent generally connected to the dataset, their function to be specified by their rdf:type</rdfs:comment>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
</rdf:Description>

<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#Agent">
	<rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
</rdf:Description>

<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#GuestAgent">
	<rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
</rdf:Description>

<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#authorizedAgent">
	<rdfs:range rdf:resource="http://dataid.dbpedia.org/ns/core#Agent"/>
	<rdfs:domain rdf:resource="http://dataid.dbpedia.org/ns/core#AuthorityEntityContext"/>
	<rdfs:label xml:lang="en">authorized agent</rdfs:label>
	<rdfs:comment xml:lang="en">Provides an agent the ability to execute authorized actions in a certain scope (e.g. to modify the metadata of a dataset)</rdfs:comment>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
</rdf:Description>

</rdf:RDF>