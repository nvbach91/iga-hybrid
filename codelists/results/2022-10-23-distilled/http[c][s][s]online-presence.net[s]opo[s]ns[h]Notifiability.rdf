<rdf:Description rdf:about="http://online-presence.net/opo/ns#Notifiability">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
</rdf:Description>

<rdf:Description rdf:about="http://online-presence.net/opo/ns#AllNotificationsPass">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://online-presence.net/opo/ns#Notifiability"/>
</rdf:Description>

<rdf:Description rdf:about="http://online-presence.net/opo/ns#Notifiability">
	<rdfs:comment xml:lang="en">The OnlinePresenceComponent used to represent the attitude
			of an Agent towards interaction with applications i.e., the
			possibility of applications to show notifications.</rdfs:comment>
	<rdfs:label xml:lang="en">Notifiability</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://online-presence.net/opo/ns#AllNotificationsPass">
	<rdfs:comment xml:lang="en">Agent can receive notifications from applications</rdfs:comment>
	<rdfs:label xml:lang="en">AllNotificationsPass</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://online-presence.net/opo/ns#Notifiability">
	<rdfs:isDefinedBy rdf:resource="http://online-presence.net/opo/ns#"/>
</rdf:Description>

<rdf:Description rdf:about="http://online-presence.net/opo/ns#AllNotificationsPass">
	<rdfs:isDefinedBy rdf:resource="http://online-presence.net/opo/ns#"/>
</rdf:Description>

<rdf:Description rdf:about="http://online-presence.net/opo/ns#NotificationsConstrained">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://online-presence.net/opo/ns#Notifiability"/>
	<rdfs:comment xml:lang="en">The acceptance of notifications from applications is somehow
			constrained (by using some rules or policies).</rdfs:comment>
	<rdfs:label xml:lang="en">NotificationsConstrained</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://online-presence.net/opo/ns#"/>
</rdf:Description>

<rdf:Description rdf:about="http://online-presence.net/opo/ns#NotificationsProhibited">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://online-presence.net/opo/ns#Notifiability"/>
	<rdfs:comment xml:lang="en">Agent cannot receive any notification form applications</rdfs:comment>
	<rdfs:label xml:lang="en">NotificationsProhibited</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://online-presence.net/opo/ns#"/>
</rdf:Description>

