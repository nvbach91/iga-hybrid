<rdf:RDF
	xmlns:vann="http://purl.org/vocab/vann/"
	xmlns:voaf="http://purl.org/vocommons/voaf#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:dcterms="http://purl.org/dc/terms/"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:dcmit="http://purl.org/dc/dcmitype/"
	xmlns:sesame="http://www.openrdf.org/schema/sesame#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
	xmlns:fn="http://www.w3.org/2005/xpath-functions#"
	xmlns:foaf="http://xmlns.com/foaf/0.1/"
	xmlns:hint="http://www.bigdata.com/queryHints#"
	xmlns:bd="http://www.bigdata.com/rdf#"
	xmlns:bds="http://www.bigdata.com/rdf/search#">

<rdf:Description rdf:about="http://www.w3.org/ns/earl">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.w3.org/ns/earl#TestMode">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.w3.org/ns/earl#automatic">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://www.w3.org/ns/earl#TestMode"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.w3.org/ns/earl#TestMode">
	<rdfs:comment xml:lang="en">describes how a test was carried out</rdfs:comment>
	<rdfs:label xml:lang="en">Test Mode</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://www.w3.org/ns/earl#automatic">
	<rdfs:comment xml:lang="en">where the test was carried out automatically by the software tool and without any human intervention</rdfs:comment>
	<rdfs:label xml:lang="en">Automatic</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://www.w3.org/ns/earl#mode">
	<rdfs:range rdf:resource="http://www.w3.org/ns/earl#TestMode"/>
	<rdfs:domain rdf:resource="http://www.w3.org/ns/earl#Assertion"/>
	<rdfs:label xml:lang="en">Mode</rdfs:label>
	<rdfs:comment xml:lang="en">mode in which the test was performed</rdfs:comment>
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.w3.org/ns/earl#TestMode">
	<rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/earl"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.w3.org/ns/earl#automatic">
	<rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/earl"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.w3.org/ns/earl#manual">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://www.w3.org/ns/earl#TestMode"/>
	<rdfs:comment xml:lang="en">where the test was carried out by human evaluators</rdfs:comment>
	<rdfs:label xml:lang="en">Manual</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/earl"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.w3.org/ns/earl#semiAuto">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://www.w3.org/ns/earl#TestMode"/>
	<rdfs:comment xml:lang="en">where the test was partially carried out by software tools, but where human input or judgment was still required to decide or help decide the outcome of the test</rdfs:comment>
	<rdfs:label xml:lang="en">Semi-Automatic</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/earl"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.w3.org/ns/earl#undisclosed">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://www.w3.org/ns/earl#TestMode"/>
	<rdfs:comment xml:lang="en">where the exact testing process is undisclosed</rdfs:comment>
	<rdfs:label xml:lang="en">Undisclosed</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/earl"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.w3.org/ns/earl#unknownMode">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://www.w3.org/ns/earl#TestMode"/>
	<rdfs:comment xml:lang="en">where the testing process is unknown or undetermined</rdfs:comment>
	<rdfs:label xml:lang="en">Unknown</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/earl"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.w3.org/ns/earl#mode">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
</rdf:Description>

</rdf:RDF>