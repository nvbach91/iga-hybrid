	<skos:inScheme rdf:resource="http://purl.org/spar/datacite/PersonalIdentifierScheme"/>
	<rdfs:comment xml:lang="en">OpenID is an open standard that describes how users can be authenticated in a decentralized manner, eliminating the need for centralized registration services. </rdfs:comment>
	<rdfs:label xml:lang="en">openid</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/spar/datacite"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/spar/datacite/orcid">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://purl.org/spar/datacite/PersonalIdentifierScheme"/>
	<rdfs:comment xml:lang="en">Open Researcher and Contributor Identifier.</rdfs:comment>
	<rdfs:label xml:lang="en">orcid</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/spar/datacite"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/spar/datacite/researcherid">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://purl.org/spar/datacite/PersonalIdentifierScheme"/>
	<rdfs:comment xml:lang="en">ResearcherID is an identifying system for scientific authors created and owned by Thomson Reuters.</rdfs:comment>
	<rdfs:label xml:lang="en">researcherid</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/spar/datacite"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/spar/datacite/social-security-number">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://purl.org/spar/datacite/PersonalIdentifierScheme"/>
	<rdfs:comment xml:lang="en">United States of America social security number scheme</rdfs:comment>
	<rdfs:label xml:lang="en">social security number</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/spar/datacite"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/spar/datacite/viaf">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://purl.org/spar/datacite/PersonalIdentifierScheme"/>
	<rdfs:comment xml:lang="en">The Virtual International Authority File is an international authority file created by amalagamating the national authority files of several national libraries and operated by the Online Computer Library Center (OCLC).  It contains information about many topics including personal names.</rdfs:comment>
	<rdfs:label xml:lang="en">viaf</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/spar/datacite"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/spar/datacite/nii">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://purl.org/spar/datacite/PersonalIdentifierScheme"/>
	<rdfs:label xml:lang="en">National Individual Identifier scheme</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/spar/datacite"/>
	<rdfs:label xml:lang="en">nii</rdfs:label>
</rdf:Description>

