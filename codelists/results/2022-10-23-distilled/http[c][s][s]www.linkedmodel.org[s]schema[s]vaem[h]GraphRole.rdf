	<skos:inScheme rdf:resource="http://www.linkedmodel.org/schema/vaem#GraphRole"/>
	<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A "vaem:CollectionGraph" is a graph that aggregates other graphs using "owl:imports" statements</rdfs:comment>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Collection graph</rdfs:label>
