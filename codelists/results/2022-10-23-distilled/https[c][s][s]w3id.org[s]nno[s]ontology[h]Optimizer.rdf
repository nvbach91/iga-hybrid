<rdf:Description rdf:about="https://w3id.org/nno/ontology#adagrad">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="https://w3id.org/nno/ontology#Optimizer"/>
	<rdfs:comment>Adagrad is an optimizer with parameter-specific learning rates, which are adapted relative to how frequently a parameter gets updated during training. The more updates a parameter receives, the smaller the learning rate.</rdfs:comment>
	<rdfs:label>Adagrad Optimizer</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="https://w3id.org/nno/ontology"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/nno/ontology#customoptimizer">
