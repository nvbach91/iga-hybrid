<rdf:Description rdf:about="https://w3id.org/nno/ontology#nadam">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="https://w3id.org/nno/ontology#Optimizer"/>
	<rdfs:comment>Much like Adam is essentially RMSprop with momentum, Nadam is Adam RMSprop with Nesterov momentum.</rdfs:comment>
	<rdfs:label>Nesterov Adam Optimizer</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="https://w3id.org/nno/ontology"/>
</rdf:Description>

