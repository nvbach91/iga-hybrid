	<rdfs:comment xml:lang="en">The responsibility to respond to contact attempts by external agents. A contact point for the entity.</rdfs:comment>
	<rdfs:label xml:lang="en">response to contact attempt</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
