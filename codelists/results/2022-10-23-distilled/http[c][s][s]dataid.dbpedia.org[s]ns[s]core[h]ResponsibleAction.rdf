<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#ResponsibleAction">
	<rdfs:comment xml:lang="en">AgentRoles provide access rights as well as responsibilities an agent has to attend to in order to fullfill this role. Responsible actions should describe actions pertaining to responsibilities an agent is supposed to do.</rdfs:comment>
	<rdfs:label xml:lang="en">responsible actions</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#AgentSupervision">
	<rdfs:comment xml:lang="en">The responsibility to supervise other agents.</rdfs:comment>
	<rdfs:label xml:lang="en">agent supervision</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#ResponsibleAction">
