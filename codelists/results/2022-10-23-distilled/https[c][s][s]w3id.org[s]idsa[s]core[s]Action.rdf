<rdf:Description rdf:about="https://w3id.org/idsa/code/READ">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="https://w3id.org/idsa/core/Action"/>
	<rdfs:comment xml:lang="en">To obtain data from the resource.</rdfs:comment>
	<rdfs:label xml:lang="en">read</rdfs:label>
	<includedIn xmlns="http://www.w3.org/ns/odrl/2/" rdf:resource="https://w3id.org/idsa/code/MODIFY"/>
</rdf:Description>

<rdf:Description rdf:about="https://w3id.org/idsa/code/MODIFY">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
