<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
	xmlns:vann="http://purl.org/vocab/vann/"
	xmlns:voaf="http://purl.org/vocommons/voaf#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:dcterms="http://purl.org/dc/terms/"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:dcmit="http://purl.org/dc/dcmitype/"
	xmlns:sesame="http://www.openrdf.org/schema/sesame#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
	xmlns:fn="http://www.w3.org/2005/xpath-functions#"
	xmlns:foaf="http://xmlns.com/foaf/0.1/"
	xmlns:hint="http://www.bigdata.com/queryHints#"
	xmlns:bd="http://www.bigdata.com/rdf#"
	xmlns:bds="http://www.bigdata.com/rdf/search#">

<rdf:Description rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.wsmo.org/ns/wsmo-lite#Message">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#EmptyMessage">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://www.wsmo.org/ns/wsmo-lite#Message"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.wsmo.org/ns/wsmo-lite#Message">
	<rdfs:comment>A Message is a instance of data that is sent by a source and that is received by a target</rdfs:comment>
	<rdfs:label>Message</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#EmptyMessage">
	<rdfs:comment>Specific message that has no content, its sole transfer achieves its purpose. For example, if an empty message is sent to the "switch light on" service of a light bulb, there is no need for an explicit content of the message asking the light to be turned on.</rdfs:comment>
	<rdfs:label>Empty message</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://www.wsmo.org/ns/wsmo-lite#hasInputMessage">
	<rdfs:range rdf:resource="http://www.wsmo.org/ns/wsmo-lite#Message"/>
	<rdfs:domain rdf:resource="http://www.wsmo.org/ns/wsmo-lite#Operation"/>
	<rdfs:label>has input message</rdfs:label>
	<rdfs:comment>Relationship between an Operation that requires some Message as its input</rdfs:comment>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.wsmo.org/ns/wsmo-lite#Message">
	<rdfs:isDefinedBy rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#EmptyMessage">
	<rdfs:isDefinedBy rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.wsmo.org/ns/wsmo-lite#hasOutputMessage">
	<rdfs:range rdf:resource="http://www.wsmo.org/ns/wsmo-lite#Message"/>
	<rdfs:domain rdf:resource="http://www.wsmo.org/ns/wsmo-lite#Operation"/>
	<rdfs:label>has output message</rdfs:label>
	<rdfs:comment>Relashionship between an Operation and the Message that it will output as the result of its processing</rdfs:comment>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
</rdf:Description>

</rdf:RDF>