	<rdfs:label xml:lang="en">Olympic Queen bed (acco:BedType)</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/acco/ns#Queen">
