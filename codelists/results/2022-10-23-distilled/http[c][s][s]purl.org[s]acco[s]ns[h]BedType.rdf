<rdf:Description rdf:about="http://purl.org/acco/ns#BedType">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/acco/ns#CaliforniaKing">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://purl.org/acco/ns#BedType"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/acco/ns#BedType">
	<rdfs:comment xml:lang="en">A type of bed. This is used for indicating the bed or beds available in an accommodation. Use DBpedia URIs whenever possible.</rdfs:comment>
	<rdfs:label xml:lang="en">Bed Type</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/acco/ns#CaliforniaKing">
	<rdfs:comment xml:lang="en">A California King or King Long bed. The actual dimension may vary by country, see http://en.wikipedia.org/wiki/Bed_size.</rdfs:comment>
	<rdfs:label xml:lang="en">California King or King Long bed (acco:BedType)</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/acco/ns#typeOfBed">
	<rdfs:range rdf:resource="http://purl.org/acco/ns#BedType"/>
	<rdfs:domain rdf:resource="http://purl.org/acco/ns#BedDetails"/>
	<rdfs:label xml:lang="en">type of bed (1..1)</rdfs:label>
	<rdfs:comment xml:lang="en">The type of bed to which the acco:BedDetail node refers, i.e. the type of bed available in the quantity indicated by acco:quantity.</rdfs:comment>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/acco/ns#BedType">
	<rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/acco/ns#CaliforniaKing">
	<rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/acco/ns#Double">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://purl.org/acco/ns#BedType"/>
	<rdfs:comment xml:lang="en">A double bed. The actual dimension may vary by country, see http://en.wikipedia.org/wiki/Bed_size.</rdfs:comment>
	<rdfs:label xml:lang="en">Double or full bed (acco:BedType)</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/acco/ns#King">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://purl.org/acco/ns#BedType"/>
	<rdfs:comment xml:lang="en">An King or Super King bed. The actual dimension may vary by country, see http://en.wikipedia.org/wiki/Bed_size.</rdfs:comment>
	<rdfs:label xml:lang="en">King or Super King bed (acco:BedType)</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns"/>
</rdf:Description>

