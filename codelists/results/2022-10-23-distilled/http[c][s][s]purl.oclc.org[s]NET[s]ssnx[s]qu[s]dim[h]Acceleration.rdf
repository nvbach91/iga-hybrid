	<rdfs:label>tendency of eastward wind due to orographic gravity wave drag</rdfs:label>
	<generalQuantityKind xmlns="http://purl.oclc.org/NET/ssnx/qu/qu#" rdf:resource="http://purl.oclc.org/NET/ssnx/qu/quantity#acceleration"/>
	<rdfs:isDefinedBy rdf:resource="http://purl.oclc.org/NET/ssnx/cf/cf-property"/>
</rdf:Description>

