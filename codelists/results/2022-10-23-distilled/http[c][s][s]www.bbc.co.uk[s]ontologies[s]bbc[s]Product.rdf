<rdf:Description rdf:about="http://www.bbc.co.uk/ontologies/bbc/Product">
	<rdfs:comment xml:lang="en-gb">Class representing products which create content about things. Within the BBC's scope, this would be one of the ten Future Media Products. Outside the BBC, this could refer to an organisation or company whose web pages we link to. Typically the official web pages of things (e.g., http://www.tomdaley.tv/) or their Wikipedia, Twitter and Facebook pages.</rdfs:comment>
	<rdfs:label xml:lang="en-gb">Product</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://www.bbc.co.uk/ontologies/bbc/Blogs">
	<rdfs:comment xml:lang="en-gb">This instance of bbc:Product is needed to associate WebDocuments from the BBC blogs.</rdfs:comment>
	<rdfs:label xml:lang="en-gb">Blogs</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://www.bbc.co.uk/ontologies/bbc/product">
	<rdfs:range rdf:resource="http://www.bbc.co.uk/ontologies/bbc/Product"/>
	<rdfs:domain rdf:resource="http://www.bbc.co.uk/ontologies/bbc/WebDocument"/>
	<rdfs:label xml:lang="en-gb">product</rdfs:label>
	<rdfs:comment xml:lang="en-gb">Associates the WebDocument with the particular BBC Product it belongs to. This reflects the editorial branding of the product.</rdfs:comment>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
</rdf:Description>

