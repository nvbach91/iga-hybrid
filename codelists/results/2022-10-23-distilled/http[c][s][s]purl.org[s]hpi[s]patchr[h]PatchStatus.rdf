<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
	xmlns:vann="http://purl.org/vocab/vann/"
	xmlns:voaf="http://purl.org/vocommons/voaf#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:dcterms="http://purl.org/dc/terms/"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:dcmit="http://purl.org/dc/dcmitype/"
	xmlns:sesame="http://www.openrdf.org/schema/sesame#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
	xmlns:fn="http://www.w3.org/2005/xpath-functions#"
	xmlns:foaf="http://xmlns.com/foaf/0.1/"
	xmlns:hint="http://www.bigdata.com/queryHints#"
	xmlns:bd="http://www.bigdata.com/rdf#"
	xmlns:bds="http://www.bigdata.com/rdf/search#">

<rdf:Description rdf:about="http://purl.org/hpi/patchr#">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/hpi/patchr#PatchStatus">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/hpi/patchr#Open">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://purl.org/hpi/patchr#PatchStatus"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/hpi/patchr#PatchStatus">
	<rdfs:comment xml:lang="en">The status of the patch, might be one of Open, Resolved, Postponed, or Rejected.</rdfs:comment>
	<rdfs:label xml:lang="en">PatchStatus</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/hpi/patchr#Open">
	<rdfs:comment xml:lang="en">The patch has been raised.</rdfs:comment>
	<rdfs:label xml:lang="en">Open</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/hpi/patchr#status">
	<rdfs:range rdf:resource="http://purl.org/hpi/patchr#PatchStatus"/>
	<rdfs:domain rdf:resource="http://purl.org/hpi/patchr#Patch"/>
	<rdfs:label xml:lang="en">status</rdfs:label>
	<rdfs:comment xml:lang="en">The status of the patch, might be one of Open, Resolved, Postponed, or Rejected.</rdfs:comment>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/hpi/patchr#PatchStatus">
	<rdfs:isDefinedBy rdf:resource="http://purl.org/hpi/patchr#"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/hpi/patchr#Open">
	<rdfs:isDefinedBy rdf:resource="http://purl.org/hpi/patchr#"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/hpi/patchr#Postponed">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://purl.org/hpi/patchr#PatchStatus"/>
	<rdfs:comment xml:lang="en">The patch has been postponed.</rdfs:comment>
	<rdfs:label xml:lang="en">Postponed</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/hpi/patchr#"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/hpi/patchr#Rejected">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://purl.org/hpi/patchr#PatchStatus"/>
	<rdfs:comment xml:lang="en">The patch has been rejected, i.e. this patch will not be applied. It can be more or less ignored but is kept for archival reasons, new requests for that patch will be rejected.</rdfs:comment>
	<rdfs:label xml:lang="en">Rejected</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/hpi/patchr#"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/hpi/patchr#Resolved">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://purl.org/hpi/patchr#PatchStatus"/>
	<rdfs:comment xml:lang="en">The patch has been resolved, i.e. this patch has been applied. It can be more or less ignored but is kept for archival reasons, new requests for that patch will be answered as resolved.</rdfs:comment>
	<rdfs:label xml:lang="en">Resolved</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/hpi/patchr#"/>
</rdf:Description>

</rdf:RDF>