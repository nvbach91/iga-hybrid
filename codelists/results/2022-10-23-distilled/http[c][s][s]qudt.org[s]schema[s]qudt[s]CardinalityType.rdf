<rdf:Description rdf:about="http://qudt.org/schema/qudt/CT_FINITE">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://qudt.org/schema/qudt/CardinalityType"/>
	<rdfs:comment rdf:datatype="http://qudt.org/schema/qudt/LatexString">Any set \\(X\\) with cardinality less than that of the natural numbers, or \\(| X | \\\\; &lt;  \\; | \\\\mathbb N | \\), is said to be a finite set.</rdfs:comment>
	<rdfs:label>Finite Cardinality Type</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://qudt.org/schema/qudt"/>
</rdf:Description>

<rdf:Description rdf:about="http://qudt.org/schema/qudt/CT_UNCOUNTABLE">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://qudt.org/schema/qudt/CardinalityType"/>
	<rdfs:comment rdf:datatype="http://qudt.org/schema/qudt/LatexString">Any set with cardinality greater than that of the natural numbers, or \\(| X | \\; &gt;  \\; | \\mathbb N | \\),  for example \\(| R| \\; =  \\;  c  \\; &gt; |\\mathbb N |\\), is said to be uncountable.</rdfs:comment>
	<rdfs:label>Uncountable Cardinality Type</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://qudt.org/schema/qudt"/>
</rdf:Description>

