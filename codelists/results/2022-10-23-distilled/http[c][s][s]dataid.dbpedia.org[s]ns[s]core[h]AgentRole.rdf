	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:dcterms="http://purl.org/dc/terms/"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:dcmit="http://purl.org/dc/dcmitype/"
	xmlns:sesame="http://www.openrdf.org/schema/sesame#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
	xmlns:fn="http://www.w3.org/2005/xpath-functions#"
	xmlns:foaf="http://xmlns.com/foaf/0.1/"
	xmlns:hint="http://www.bigdata.com/queryHints#"
	xmlns:bd="http://www.bigdata.com/rdf#"
	xmlns:bds="http://www.bigdata.com/rdf/search#">

<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
</rdf:Description>

<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#AgentRole">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
</rdf:Description>

<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#Contact">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://dataid.dbpedia.org/ns/core#AgentRole"/>
</rdf:Description>

<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#AgentRole">
	<rdfs:comment xml:lang="en">Instances of this class define an array of rights and responsibilities an agent, assigned with this role for a given scope of entities, has to address or can execute.</rdfs:comment>
	<rdfs:label xml:lang="en">Agent role</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#Contact">
	<rdfs:comment xml:lang="en">Contact agent. An agent that can be contacted for general requests about the resource.</rdfs:comment>
	<rdfs:label xml:lang="en">contact</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#authorityAgentRole">
	<rdfs:range rdf:resource="http://dataid.dbpedia.org/ns/core#AgentRole"/>
	<rdfs:domain rdf:resource="http://dataid.dbpedia.org/ns/core#AuthorityEntityContext"/>
	<rdfs:label xml:lang="en">authority agent role</rdfs:label>
	<rdfs:comment xml:lang="en">Adds a role an agent can administer in the scope provided by the AuthorityEntityContext, thereby allowing for certain actions an agent can execute.</rdfs:comment>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
</rdf:Description>

<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#AgentRole">
	<rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
</rdf:Description>

<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#Contact">
	<rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
</rdf:Description>

<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#Contributor">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://dataid.dbpedia.org/ns/core#AgentRole"/>
	<rdfs:comment xml:lang="en">Contributor to the resource. An agent that was involved in creating or maintaining the resource but does not have the main part in this activity.</rdfs:comment>
	<rdfs:label xml:lang="en">contributor</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
</rdf:Description>

<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#Creator">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://dataid.dbpedia.org/ns/core#AgentRole"/>
	<rdfs:comment xml:lang="en">Creator of the resource. An agent that is credited with a main part in the initial creation of the resource.</rdfs:comment>
	<rdfs:label xml:lang="en">creator</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
</rdf:Description>

<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#Guest">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://dataid.dbpedia.org/ns/core#AgentRole"/>
	<rdfs:comment xml:lang="en">A visitor/anonymous agent has only read rights on public and semi-private documents</rdfs:comment>
	<rdfs:label xml:lang="en">guest</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
</rdf:Description>

<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#Maintainer">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://dataid.dbpedia.org/ns/core#AgentRole"/>
	<rdfs:comment xml:lang="en">Maintainer of the dataset. An agent that ensures the technical correctness, accessibility and up-to-dateness of a dataset.</rdfs:comment>
	<rdfs:label xml:lang="en">maintainer</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
</rdf:Description>

<rdf:Description rdf:about="http://dataid.dbpedia.org/ns/core#Publisher">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://dataid.dbpedia.org/ns/core#AgentRole"/>
	<rdfs:comment xml:lang="en">Publisher of the dataset. An agent that makes the dataset accessible online on a server or repository without necessarily being involved in its creation.</rdfs:comment>
	<rdfs:label xml:lang="en">publisher</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://dataid.dbpedia.org/ns/core#"/>
</rdf:Description>

</rdf:RDF>