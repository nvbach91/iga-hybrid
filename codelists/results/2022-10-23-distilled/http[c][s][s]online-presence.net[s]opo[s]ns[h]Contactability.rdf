<rdf:Description rdf:about="http://online-presence.net/opo/ns#">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
</rdf:Description>

<rdf:Description rdf:about="http://online-presence.net/opo/ns#Contactability">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
</rdf:Description>

<rdf:Description rdf:about="http://online-presence.net/opo/ns#ConstrainedContactability">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://online-presence.net/opo/ns#Contactability"/>
</rdf:Description>

<rdf:Description rdf:about="http://online-presence.net/opo/ns#Contactability">
	<rdfs:comment xml:lang="en">OnlineStatusComponent used to represent the state of
			contactability of an Agent. Contatctability relates to the
			possiblity of other Agents to conatct the Agent freely or
			only with regard to some conditions.</rdfs:comment>
	<rdfs:label xml:lang="en">Contactability</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://online-presence.net/opo/ns#ConstrainedContactability">
	<rdfs:comment xml:lang="en">The Agent cannot be contated by anyone on the Web, but the
			contactability is controled by some rules/policies.</rdfs:comment>
	<rdfs:label xml:lang="en">ConstrainedContactability</rdfs:label>
</rdf:Description>

<rdf:Description rdf:about="http://online-presence.net/opo/ns#Contactability">
	<rdfs:isDefinedBy rdf:resource="http://online-presence.net/opo/ns#"/>
</rdf:Description>

<rdf:Description rdf:about="http://online-presence.net/opo/ns#ConstrainedContactability">
	<rdfs:isDefinedBy rdf:resource="http://online-presence.net/opo/ns#"/>
</rdf:Description>

<rdf:Description rdf:about="http://online-presence.net/opo/ns#FreelyContactable">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://online-presence.net/opo/ns#Contactability"/>
	<rdfs:comment xml:lang="en">The Agent can be contacted by anyone on the Web</rdfs:comment>
	<rdfs:label xml:lang="en">FreelyContactable</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://online-presence.net/opo/ns#"/>
</rdf:Description>

