	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://purl.org/spar/scoro/ProjectRole"/>
	<rdfs:comment xml:lang="en">The role a person or an institution has of being project leader for a project such as a research project.</rdfs:comment>
	<rdfs:label xml:lang="en">project leader</rdfs:label>
	<rdfs:isDefinedBy rdf:resource="http://purl.org/spar/scoro/"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/spar/scoro/project-manager">
