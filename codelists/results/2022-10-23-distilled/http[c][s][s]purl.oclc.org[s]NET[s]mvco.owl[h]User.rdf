<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
	xmlns:vann="http://purl.org/vocab/vann/"
	xmlns:voaf="http://purl.org/vocommons/voaf#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:dcterms="http://purl.org/dc/terms/"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:dcmit="http://purl.org/dc/dcmitype/"
	xmlns:sesame="http://www.openrdf.org/schema/sesame#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
	xmlns:fn="http://www.w3.org/2005/xpath-functions#"
	xmlns:foaf="http://xmlns.com/foaf/0.1/"
	xmlns:hint="http://www.bigdata.com/queryHints#"
	xmlns:bd="http://www.bigdata.com/rdf#"
	xmlns:bds="http://www.bigdata.com/rdf/search#">

<rdf:Description rdf:about="http://purl.oclc.org/NET/mvco.owl">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.oclc.org/NET/mvco.owl#User">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.oclc.org/NET/mvco.owl#Anonymous">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://purl.oclc.org/NET/mvco.owl#User"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.oclc.org/NET/mvco.owl#User">
	<rdfs:comment xml:lang="en">Any person or legal entity in a Value-Chain connecting (and including) Creator and End-User.</rdfs:comment>
</rdf:Description>

<rdf:Description rdf:about="http://purl.oclc.org/NET/mvco.owl#actedBy">
	<rdfs:range rdf:resource="http://purl.oclc.org/NET/mvco.owl#User"/>
	<rdfs:domain rdf:resource="http://purl.oclc.org/NET/mvco.owl#Action"/>
	<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Role who has executed the Action</rdfs:comment>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.oclc.org/NET/mvco.owl#User">
	<rdfs:isDefinedBy rdf:resource="http://purl.oclc.org/NET/mvco.owl"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.oclc.org/NET/mvco.owl#Anonymous">
	<rdfs:isDefinedBy rdf:resource="http://purl.oclc.org/NET/mvco.owl"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.oclc.org/NET/mvco.owl#actOnBehalfOf">
	<rdfs:range rdf:resource="http://purl.oclc.org/NET/mvco.owl#User"/>
	<rdfs:domain rdf:resource="http://purl.oclc.org/NET/mvco.owl#ContentHandler"/>
	<rdfs:comment xml:lang="en">Relates a ContentHandler with the User under the auspices of which the ContentHandler operates.</rdfs:comment>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.oclc.org/NET/mvco.owl#hasRightsOwner">
	<rdfs:range rdf:resource="http://purl.oclc.org/NET/mvco.owl#User"/>
	<rdfs:domain rdf:resource="http://purl.oclc.org/NET/mvco.owl#IPEntity"/>
	<rdfs:comment xml:lang="en">Defines the owner of the Rights over an IP Entity.</rdfs:comment>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.oclc.org/NET/mvco.owl#issuedBy">
	<rdfs:range rdf:resource="http://purl.oclc.org/NET/mvco.owl#User"/>
	<rdfs:domain rdf:resource="http://purl.oclc.org/NET/mvco.owl#Permission"/>
	<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Declares who has issued a permission</rdfs:comment>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
</rdf:Description>

</rdf:RDF>