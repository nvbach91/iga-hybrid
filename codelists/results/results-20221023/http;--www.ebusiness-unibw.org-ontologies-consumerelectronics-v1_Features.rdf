<rdf:RDF
    xmlns:dct="http://purl.org/dc/terms/"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dcmit="http://purl.org/dc/dcmitype/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:voaf="http://purl.org/vocommons/voaf#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:vann="http://purl.org/vocab/vann/">
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#TextViewing">
    <rdfs:label xml:lang="en">Text viewing</rdfs:label>
    <rdfs:label xml:lang="de">Textdarstellung</rdfs:label>
    <rdfs:comment xml:lang="en">Ability to display text.</rdfs:comment>
    <skos:inScheme>
      <skos:ConceptScheme rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
    </skos:inScheme>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Clock">
    <rdfs:label xml:lang="de">Uhr</rdfs:label>
    <rdfs:label xml:lang="en">Clock</rdfs:label>
    <rdfs:comment xml:lang="en">Can display the current time.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Flash">
    <rdfs:label xml:lang="en">Flash</rdfs:label>
    <rdfs:label xml:lang="de">Blitz</rdfs:label>
    <rdfs:comment xml:lang="en">Is a device used in photography that produces an instantaneous flash of artificial light.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#SelfTimer">
    <rdfs:label xml:lang="en">Self-timer</rdfs:label>
    <rdfs:label xml:lang="de">Selbstauslöser</rdfs:label>
    <rdfs:comment xml:lang="en">Ability to give a delay between the pressing of the shutter release and the shutter firing.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Radio">
    <rdfs:label xml:lang="de">Radio</rdfs:label>
    <rdfs:label xml:lang="en">Radio</rdfs:label>
    <rdfs:comment xml:lang="en">Ability to recieve radiosignal.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#BacklightMode">
    <rdfs:label xml:lang="en">Backlight mode</rdfs:label>
    <rdfs:label xml:lang="de">Hintergrundbeleuchtungsmodus</rdfs:label>
    <rdfs:comment xml:lang="en">Ability to photograph against a light source.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#DocumentViewing">
    <rdfs:label xml:lang="de">Dokumentdarstellung</rdfs:label>
    <rdfs:label xml:lang="en">Document viewing</rdfs:label>
    <rdfs:comment xml:lang="en">Ability to display documents.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#EconomicalMode">
    <rdfs:label xml:lang="de">Energiesparmodus</rdfs:label>
    <rdfs:label xml:lang="en">Economical mode</rdfs:label>
    <rdfs:comment xml:lang="en">Equipment that allows economical printing.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#RedEyeReduction">
    <rdfs:label xml:lang="en">Red eye reduction</rdfs:label>
    <rdfs:label xml:lang="de">Rote-Augen-Effekt Reduktion</rdfs:label>
    <rdfs:comment xml:lang="en">Defines the ability to precede the flash with a series of short, low-power flashes, or a continuous piercing bright light triggering the iris to contract.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#AudioInput">
    <rdfs:label xml:lang="de">Audioeingang</rdfs:label>
    <rdfs:label xml:lang="en">Audio input</rdfs:label>
    <rdfs:comment xml:lang="en">Defines the support of a computer soundcard driver protocol for digital audio specified by Steinberg, providing a low-latency and high fidelity interface between a software application and a computer's sound card.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Alert">
    <rdfs:label xml:lang="de">Alarm</rdfs:label>
    <rdfs:label xml:lang="en">Alert</rdfs:label>
    <rdfs:comment xml:lang="en">Has wakeup-function.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#LightExposureControl">
    <rdfs:label xml:lang="de">Belichtungsregelung</rdfs:label>
    <rdfs:label xml:lang="en">Light exposure control</rdfs:label>
    <rdfs:comment xml:lang="en">Is the total amount of light allowed to fall on the photographic medium during the process of taking a photograph.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Snapshot">
    <rdfs:label xml:lang="en">Snapshot</rdfs:label>
    <rdfs:label xml:lang="de">Snapshot</rdfs:label>
    <rdfs:comment xml:lang="en">Is a shot that is aimed and fired very quickly at a target that appears suddenly and for a very short period of time.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#VoiceRecording">
    <rdfs:label xml:lang="en">Voicerecorder</rdfs:label>
    <rdfs:label xml:lang="de">Voicerekorder</rdfs:label>
    <rdfs:comment xml:lang="en">Ability to record sounds with a micro.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#ColorPrint">
    <rdfs:label xml:lang="de">Farbdruck</rdfs:label>
    <rdfs:label xml:lang="en">Color print</rdfs:label>
    <rdfs:comment xml:lang="en">The reproduction of an image or text in color on a paper.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#FaceIdendification">
    <rdfs:label xml:lang="en">Face identification</rdfs:label>
    <rdfs:label xml:lang="de">Gesichtserkennung</rdfs:label>
    <rdfs:comment xml:lang="en">Ability to identify the face and to justify the picture to it.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Microphon">
    <rdfs:label xml:lang="en">Microphon</rdfs:label>
    <rdfs:label xml:lang="de">Mikrofon</rdfs:label>
    <rdfs:comment xml:lang="en">Ability to record voices and sounds.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#LineInRecorder">
    <rdfs:label xml:lang="de">Audioaufnahme</rdfs:label>
    <rdfs:label xml:lang="en">Line in recorder</rdfs:label>
    <rdfs:comment xml:lang="en">Ability to record sounds via cable connection.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#PictureViewing">
    <rdfs:label xml:lang="en">Picture viewing</rdfs:label>
    <rdfs:label xml:lang="de">Bildbetrachtung</rdfs:label>
    <rdfs:comment xml:lang="en">Ability to display digital pictures.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#VideoPlayBack">
    <rdfs:label xml:lang="de">Videowiedergabe</rdfs:label>
    <rdfs:label xml:lang="en">Video play back</rdfs:label>
    <rdfs:comment xml:lang="en">Ability to display video files.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#DuplexPrint">
    <rdfs:label xml:lang="en">Duplex print</rdfs:label>
    <rdfs:label xml:lang="de">Duplexdruck</rdfs:label>
    <rdfs:comment xml:lang="en">A feature of computer printers and MFPs that allows the automatic printing of a sheet of paper on both sides.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Shuffle">
    <rdfs:label xml:lang="de">Zufällige Wiedergabe</rdfs:label>
    <rdfs:label xml:lang="en">Shuffle</rdfs:label>
    <rdfs:comment xml:lang="en">Ability to play files in random order.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Playlist">
    <rdfs:label xml:lang="de">Wiedergabeliste</rdfs:label>
    <rdfs:label xml:lang="en">Playlist</rdfs:label>
    <rdfs:comment xml:lang="en">Ability to put audiofiles in a playlist.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#VideoRecording">
    <rdfs:label xml:lang="en">Video recording</rdfs:label>
    <rdfs:label xml:lang="de">Videoaufnahme</rdfs:label>
    <rdfs:comment xml:lang="en">Ability to record videos.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#WindFilter">
    <rdfs:label xml:lang="en">Wind filter</rdfs:label>
    <rdfs:label xml:lang="de">Windfilter</rdfs:label>
    <rdfs:comment xml:lang="en">Ability to filter out wind noises.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#AutomaticPictureStabilizer">
    <rdfs:label xml:lang="en">Automatic picture stabilizer</rdfs:label>
    <rdfs:label xml:lang="de">Automatischer Bildstabilisator</rdfs:label>
    <rdfs:comment xml:lang="en">A device used to stabilize a picture in order to keep it in a steady position.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Features"/>
  </skos:Concept>
</rdf:RDF>
