<rdf:RDF
    xmlns:dct="http://purl.org/dc/terms/"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dcmit="http://purl.org/dc/dcmitype/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:voaf="http://purl.org/vocommons/voaf#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:vann="http://purl.org/vocab/vann/">
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#IncredibleSurround">
    <rdfs:label xml:lang="de">Incredible Surround</rdfs:label>
    <rdfs:label xml:lang="en">Incredible Surround</rdfs:label>
    <rdfs:comment xml:lang="en">Psycho-acoustic method of widening the stereo spatial image especially suited for applications with limited separation of left and right speakers.</rdfs:comment>
    <skos:inScheme>
      <skos:ConceptScheme rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#AudioFeatures"/>
    </skos:inScheme>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#Equalizer">
    <rdfs:label xml:lang="de">Equalizer</rdfs:label>
    <rdfs:label xml:lang="en">Equalizer</rdfs:label>
    <rdfs:comment xml:lang="en">Equalization, equalisation or EQ is the process of using passive or active electronic elements or digital algorithms for the purpose of altering (originally flattening) the frequency response characteristics of a system.

(This description is partly based on or reuses content from the Wikipedia article "Equalization" (http://en.wikipedia.org/wiki/Equalization) and is licensed under the GNU Free Documentation License (http://www.gnu.org/copyleft/fdl.html).)</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#AudioFeatures"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#DynamicBassEnhancement">
    <rdfs:label xml:lang="en">Dynamic bass enhancement</rdfs:label>
    <rdfs:label xml:lang="de">Dynamischer Bass</rdfs:label>
    <rdfs:comment xml:lang="en">Dynamic settlement of bass audiosignals.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#AudioFeatures"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#SmartSoundControl">
    <rdfs:label xml:lang="de">SmartSound control</rdfs:label>
    <rdfs:label xml:lang="en">SmartSound control</rdfs:label>
    <rdfs:comment xml:lang="en">SmartSound’s technology benefits television producers and editors by giving enhanced creative options through special music technology that delivers multiple musical variations for different moods.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#AudioFeatures"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#AutoVolumeLeveller">
    <rdfs:label xml:lang="en">Auto volume leveller</rdfs:label>
    <rdfs:label xml:lang="de">Automatische Lautstärkenanpassung</rdfs:label>
    <rdfs:comment xml:lang="en">Levels the audio volume of televisions automatically so that undesired peaks in the sound can be reduced.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#AudioFeatures"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#BuiltInSpeakers">
    <rdfs:label xml:lang="en">Built in speakers</rdfs:label>
    <rdfs:label xml:lang="de">Eingebaute Lautsprecher</rdfs:label>
    <rdfs:comment xml:lang="en">Speakers that are integrated in the hull of a electronical device, that is able to play audio signals.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#AudioFeatures"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#NoiseReduction">
    <rdfs:label xml:lang="de">Geräuschunterdrückung</rdfs:label>
    <rdfs:label xml:lang="en">Noise reduction</rdfs:label>
    <rdfs:comment xml:lang="en">Noise reduction is the process of removing noise from a signal.</rdfs:comment>
    <skos:inScheme rdf:resource="http://www.ebusiness-unibw.org/ontologies/consumerelectronics/v1#AudioFeatures"/>
  </skos:Concept>
</rdf:RDF>
