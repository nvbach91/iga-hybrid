      <skos:ConceptScheme rdf:about="http://purl.org/opdm/refrigerator#TypeOfBody"/>
    </skos:inScheme>
  </skos:Concept>
  <skos:Concept rdf:about="http://purl.org/opdm/refrigerator#BuiltIn">
    <rdfs:label xml:lang="en">BuiltIn</rdfs:label>
    <rdfs:comment xml:lang="en">It represents a bult-in type of body frame for a fridge/freezer.</rdfs:comment>
    <skos:inScheme rdf:resource="http://purl.org/opdm/refrigerator#TypeOfBody"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://purl.org/opdm/refrigerator#FreeStanding">
    <rdfs:label xml:lang="en">FreeStanding</rdfs:label>
    <rdfs:comment xml:lang="en">It represents a free-standing type of body frame for a fridge/freezer.</rdfs:comment>
